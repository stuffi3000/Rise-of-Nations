			allowed = {
				TAG = ARG
			}

			available = {

				ARG = { has_completed_focus = ra_ram_ft_ecp_gotw }

				OR = {
					AND = {
						has_idea = ra_arg_ger_trade_deal
						has_idea = ra_arg_sov_trade_deal
						has_idea = ra_arg_jap_trade_deal
					}
					AND = {
						has_idea = ra_arg_sov_trade_deal
						has_idea = ra_arg_jap_trade_deal
						has_idea = ra_arg_eng_trade_deal
					}
					AND = {
						has_idea = ra_arg_jap_trade_deal
						has_idea = ra_arg_eng_trade_deal
						has_idea = ra_arg_ger_trade_deal
					}
					AND = {
						has_idea = ra_arg_eng_trade_deal
						has_idea = ra_arg_ger_trade_deal
						has_idea = ra_arg_sov_trade_deal
					}
				}
			}


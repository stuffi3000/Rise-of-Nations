	all_state = { if = { limit = { is_core_of = TAN } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = MAN } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = DPK } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = POL } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = DDR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = CZE } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = HUN } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = ROM } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = BUL } is_controlled_by = ROOT } }

}

#Greater USA

RON_Formable_Decision105_trigger = {

	all_state = { if = { limit = { is_core_of = USA } is_controlled_by = ROOT } }

	#Mexican Border States
	482 = { is_controlled_by = ROOT }
	483 = { is_controlled_by = ROOT }
	484 = { is_controlled_by = ROOT }

	#Canada Border States
	471 = { is_controlled_by = ROOT }
	473 = { is_controlled_by = ROOT }

}

#USA-Canada

RON_Formable_Decision106_trigger = {

	all_state = { if = { limit = { is_core_of = USA } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = CAN } is_controlled_by = ROOT } }

}

#Greater Austria

RON_Formable_Decision107_trigger = {

	all_state = { if = { limit = { is_core_of = AUS NOT = { is_core_of = ITA } } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = CZR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = SLV } is_controlled_by = ROOT } }

}

#Modern Greater Germany
RON_Formable_Decision108_trigger = {

	all_state = { if = { limit = { is_core_of = WGR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = DDR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = AUS NOT = { is_core_of = ITA } } is_controlled_by = ROOT } }
	#Swiss German States
	3 = { is_controlled_by = ROOT }
	151 = { is_controlled_by = ROOT }

}

#Germanic Union
RON_Formable_Decision109_trigger = {

	all_state = { if = { limit = { is_core_of = WGR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = DDR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = AUS } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = HOL } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = LUX } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = NOR } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = SWE } is_controlled_by = ROOT } }
	all_state = {
		if = {
			limit = {
				is_core_of = DEN
				NOT = { is_core_of = ICE }
				NOT = { is_core_of = GRL }
			}
			is_controlled_by = ROOT
		}
	}


}

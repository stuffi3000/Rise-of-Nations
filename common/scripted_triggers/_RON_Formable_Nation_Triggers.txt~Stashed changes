#Greater Germany
RON_Formable_Decision0_trigger = {

	#German Empire States
	all_state = {
		if = {
			limit = {
				is_core_of = WGR
			}
			is_controlled_by = ROOT
		}
	}

	all_state = {
		if = {
			limit = {
				is_core_of = DDR
			}
			is_controlled_by = ROOT
		}
	}

	all_state = {
		if = {
			limit = {
				is_core_of = PRS
			}
			is_controlled_by = ROOT
		}
	}

	#Polish Corridor
	85 = { is_controlled_by = ROOT }
	86 = { is_controlled_by = ROOT }
	865 = { is_controlled_by = ROOT }
	762 = { is_controlled_by = ROOT }

	#Memel
	188 = { is_controlled_by = ROOT }

	#Alsace
	28 = { is_controlled_by = ROOT }

	#Austria
	all_state = {
		if = {
			limit = {
				is_core_of = AUS
				NOT = { is_core_of = ITA }
			}
			is_controlled_by = ROOT
		}
	}

	#Sudetenland
	69 = { is_controlled_by = ROOT }
	74 = { is_controlled_by = ROOT }
	884 = { is_controlled_by = ROOT }
	886 = { is_controlled_by = ROOT }
	887 = { is_controlled_by = ROOT }
	888 = { is_controlled_by = ROOT }

}


	all_state = { if = { limit = { is_core_of = ITA } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = ETH } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = LBA } is_controlled_by = ROOT } }

	any_controlled_state = {
		is_core_of = CRO
		is_controlled_by = ROOT
	}

	any_controlled_state = {
		is_core_of = YUG
		is_controlled_by = ROOT
	}

	any_controlled_state = {
		is_core_of = ALB
		is_controlled_by = ROOT
	}

	any_controlled_state = {
		is_core_of = GRE
		is_controlled_by = ROOT
	}

	any_controlled_state = {
		is_core_of = TUR
		is_controlled_by = ROOT
	}
}

#Greater Peru
RON_Formable_Decision100_trigger = {

	all_state = { if = { limit = { is_core_of = PRU } is_controlled_by = ROOT } }
	all_state = { if = { limit = { is_core_of = BOL NOT = { is_core_of = PAR } } is_controlled_by = ROOT } }

	#Ecuador
	490 = { is_controlled_by = ROOT }
	305 = { is_controlled_by = ROOT }

}

#Southern Cone

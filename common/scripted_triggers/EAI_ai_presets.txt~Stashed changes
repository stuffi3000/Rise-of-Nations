######## Heavy Cruiser Focus ########

EAI_Heavy_Cruiser_Focus_1_PRESET = {
	if = { limit = { NOT = { has_global_flag = EAI_challenging_mode } }

		OR = {
			AND = {
				is_major = no
				NOT = { EAI_asian_minor = yes }
			}
		}
	}

	else_if = { limit = { has_global_flag = EAI_challenging_mode }

		OR = {
			AND = {
				is_major = no
				NOT = { EAI_asian_minor = yes }
			}
		}
	}
}

EAI_Heavy_Cruiser_Focus_0_PRESET = { # default
	OR = {
		always = no
	}
}


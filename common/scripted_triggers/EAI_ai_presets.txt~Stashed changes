######## Destroyer Focus ########

EAI_Destroyer_Focus_3_PRESET = { # cheap design
	if = { limit = { NOT = { has_global_flag = EAI_challenging_mode } }

		OR = {
			original_tag = ITA
			original_tag = FRA
			original_tag = SOV

			AND = {
				num_of_naval_factories > 2
				NOT = {
					original_tag = GER
					original_tag = ITA
					original_tag = JAP
					original_tag = USA
					original_tag = ENG
					original_tag = FRA
					original_tag = SOV
				}
			}
		}
	}

	else_if = { limit = { has_global_flag = EAI_challenging_mode }

		OR = {
			original_tag = GER
			original_tag = ITA
			original_tag = JAP
			original_tag = USA
			original_tag = ENG
			original_tag = FRA
			original_tag = SOV
			num_of_naval_factories > 2
		}
	}
}

EAI_Destroyer_Focus_2_PRESET = { # escort+asw hybrid

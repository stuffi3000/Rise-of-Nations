true_blitzkrieg = {
	id = 12

	possible = {
		is_ironman = yes
		difficulty > 1
		has_start_date < 1936.01.02
		has_any_custom_difficulty_setting = no
		game_rules_allow_achievements = yes
		tag = GER
	}

	happened = {
		date < 1940.01.01
		FRA = {
			OR = {
				exists = no
				has_capitulated = yes
				has_global_flag = achievement_france_surrender
			}
		}
		POL = {
			OR = {
				exists = no
				has_capitulated = yes
			}
		}
	}
}


	#### WW1 TECHS

	early_fighter_1914 = {

		enable_equipments = {
			early_fighter_equipment_0
		}

		path = {
			leads_to_tech = basic_fighter
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1914

		folder = {
			name = air_techs_folder
			position = { x = @fighter y = @1914 }
		}

		categories = {
			light_air
			light_fighter
			air_equipment
		}

		ai_will_do = {
			factor = 2

			modifier = {
				factor = 2
				is_major = yes
			}

			modifier = {
				factor = 1.2
				date > "1915.1.1"
			}

			modifier = {
				factor = 1.2
				date > "1915.1.1"
			}

			modifier = {
				factor = 1.2
				date > "1917.1.1"
			}

			modifier = {
				factor = 5
				date > "1918.1.1"
			}
		}
	}


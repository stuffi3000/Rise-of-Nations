	#### Helicopter Tree


	early_helicopter = {

		on_research_complete = {
			custom_effect_tooltip = helicopter_tt
		}

		desc = "HELICOPTER_SPECIAL"

		path = {
			leads_to_tech = cargo_helicopter_1
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = attack_helicopter_1
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = scout_helicopter_1
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1950

		folder = {
			name = air_techs_folder
			position = { x = @scout_helicopter y = @1945 }
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1949.6.1 EAI_armor_tech_focus = yes }
			modifier = { factor = 0 date < 1950.1.1 NOT = { EAI_armor_tech_focus = yes } }

			#WEIGHT
			modifier = { factor = 2 EAI_armor_tech_focus = yes }

		}

		categories = {
			helicopter
		}
	}



	early_fighter = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | AIR: early_fighter" } }

		enable_equipments = {
			fighter_equipment_0
		}

		path = {
			leads_to_tech = fighter1
			research_cost_coeff = 1
		}



		research_cost = 2
		start_year = 1928
		folder = {
			name = air_techs_folder
			position = { x = @fighter y = @1928 }
		}

		categories = {
			light_air
			light_fighter
			air_equipment
		}

		sub_technologies = {
			cv_early_fighter
		}

		ai_will_do = { #EAI
			factor = 1

			#WEIGHT
			modifier = {
				factor = 150
				num_of_military_factories > 25
			}
		}
	}

	cv_early_fighter = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | AIR: cv_early_fighter" } }


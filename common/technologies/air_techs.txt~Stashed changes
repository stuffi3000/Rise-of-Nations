			modifier = { factor = 0 NOT = { has_tech = basic_carrier has_tech = basic_ship_hull_carrier EAI_NAV_tech_focus = yes } }

			modifier = { factor = 0 EAI_FTR_tech_focus = yes NOT = { has_tech = fighter1 is_researching_technology = fighter1 } }
		}
	}


	RON_Fighter_1490 = {

		on_research_complete = {
			custom_effect_tooltip = ron_required_tech_tt
		}

		desc = "RON_REQUIRED_TECH_SPECIAL"

		path = {
			leads_to_tech = RON_Fighter_1890
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = RON_Zepellin_1901
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1490
		folder = {
			name = air_techs_folder
			position = { x = @fighter y = @1490 }
		}

		categories = {
			air_equipment
		}

		ai_will_do = {
			factor = 1
		}
	}


	jet_strategic_bomber_a = {

		on_research_complete_limit = {
			is_ai = yes has_air_experience > 130
		}

		on_research_complete = { #EAI
			hidden_effect = { country_event = { id = EAI_V_STRAT.5 hours = 1 } } #upgrade this instantly so that the AI doesn't waste prod.eff by having to do two production switches
		}

		enable_equipments = {
			jet_strat_bomber_equipment_2
		}

		research_cost = 2
		start_year = 1955


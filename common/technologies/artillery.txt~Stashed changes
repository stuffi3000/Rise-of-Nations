	basic_heavy_artillery = {

		enable_equipments = {
			heavy_artillery_equipment_1
		}

		research_cost = 1.5

		start_year = 1930

		folder = {
			name = artillery_folder
			position = { x = @heavy_arty y = @1930 }
		}

		path = {
			leads_to_tech = heavy_artillery_upgrade1
			research_cost_coeff = 1
		}

		categories = {
			artillery
		}

		ai_will_do = { #EAI
			factor = 1

			#DATE
			modifier = { factor = 0 date < 1931.1.1 }

			#WEIGHT
			modifier = { factor = 20 date > 1931.1.1 }
			modifier = { factor = 2 EAI_artillery_tech_focus = yes }
		}
	}

	heavy_artillery_upgrade1 = {
		ww1_heavy_artillery_support = {
			soft_attack = 0.1
		}
		heavy_artillery_brigade = {
			soft_attack = 0.1
		}

		path = {
			leads_to_tech = improved_heavy_artillery
			research_cost_coeff = 1
		}

		research_cost = 1.5

		start_year = 1936

		folder = {
			name = artillery_folder

				factor = 20
				OR = {
					date > 1937.6.1
					AND = { is_major = yes any_enemy_country = { is_major = yes } }
				}
			}

			modifier = { factor = 2 }
		}

		categories = {
			artillery
			cat_anti_tank
		}

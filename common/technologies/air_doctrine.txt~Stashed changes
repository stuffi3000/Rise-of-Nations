	### Cold War Air Doctrines

	RON_Post_WW2_Air = {

		doctrine = yes
		research_cost = 2
		start_year = 1946

		categories = {
			air_doctrine
		}

		on_research_complete = {
			custom_effect_tooltip = cwdoctrines_tt
		}

		path = {
			leads_to_tech = RON_CW_Patrol_Sectors
			research_cost_coeff = 1
		}

		desc = "CWDOCTRINES_SPECIAL"

		folder = {
			name = air_doctrine_folder
			position = { x = 9 y = 40 }
		}

		ai_will_do = {
			factor = 5
		}

	}


	basic_heavy_tank = { #Grosstraktor

		enable_equipments = {
			heavy_tank_equipment_1
		}

		on_research_complete = {
			if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ARMOR: basic_heavy_tank" }
			if = {
				limit = {
					NOT = { has_template_containing_unit = light_armor }
					NOT = { has_template_containing_unit = heavy_armor }
					NOT = { has_template_containing_unit = medium_armor }
				}
				hidden_effect = {
					if = {
						limit = {
							has_tech = motorised_infantry
						}

						load_oob = "unlock_tanks_mot"
					}
					if = {
						limit = {
							NOT = { has_tech = motorised_infantry }
						}

						load_oob = "unlock_tanks"
					}
				}

				custom_effect_tooltip = UNLOCK_DIVISION_TEMPLATE
			}
		}

		path = {
			leads_to_tech = improved_heavy_tank
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = early_infantry_tank
			research_cost_coeff = 0.9
		}




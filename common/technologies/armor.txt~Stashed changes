	tank_engine = {

		on_research_complete = {
			custom_effect_tooltip = ron_required_tech_tt
		}

		desc = "RON_REQUIRED_TECH_SPECIAL"

		# No Effect

		path = {
			leads_to_tech = tank_armor
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = tank_gun
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1916
		folder = {
			name = armour_folder
			position = { x = @assault_tank y = @1916 }
		}

		ai_will_do = {
			factor = 1
		}

		categories = {
			armor
			cat_light_armor
		}
	}


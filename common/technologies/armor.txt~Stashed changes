	advanced_light_tank_a = {

		enable_equipments = {
			light_tank_equipment_4
		}

		sub_technologies = {
			advanced_light_tank_td_a
			advanced_light_tank_art_a
			advanced_light_tank_spaa_a
		}

		path = {
			leads_to_tech = advanced_light_tank_b
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = main_battle_tank_a
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1945

		folder = {
			name = armour_folder
			position = { x = @light_tank y = @1945 }
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1944.6.1 EAI_armor_tech_focus = yes }
			modifier = { factor = 0 date < 1944.6.1 NOT = { EAI_armor_tech_focus = yes } }

			#WEIGHT
			modifier = { factor = 2 EAI_armor_tech_focus = yes }

			#RESTRICTIONS
			modifier = { factor = 0 NOT = { AND = { EAI_LARM_tech_focus = yes check_variable = { EAI_FOCUS/LAND/armor_focus = global.EAI_AT_FOCUS } check_variable = { EAI_FOCUS/LAND/attachment = global.EAI_LARM_ATTACHMENT } } } }
		}

		categories = {
			armor
			cat_light_armor
		}

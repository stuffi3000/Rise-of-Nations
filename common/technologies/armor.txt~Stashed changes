	motorised_infantry = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | INFANTRY: motorised_infantry" } }


		enable_equipments = {
			motorized_equipment_1
		}


		path = {
			leads_to_tech = mechanised_infantry
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = motorised_infantry2
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1936

		folder = {
			name = armour_folder
			position = { x = @motorized y = @1936 }
		}

		categories = {
			motorized_equipment
			infantry_tech
		}

		ai_will_do = { #EAI
			factor = 1

			#DATE
			modifier = { factor = 0 date < 1937.1.1 }

			#WEIGHT
			modifier = { factor = 20 date > 1937.1.1 }
		}
	}


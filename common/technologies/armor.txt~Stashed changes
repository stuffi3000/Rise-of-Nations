	basic_medium_tank = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ARMOR: basic_medium_tank" } } #Panzer III

		enable_equipments = {

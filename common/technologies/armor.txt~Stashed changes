	advanced_heavy_tank = { #King Tiger

		on_research_complete = { #EAI
			if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ARMOR: advanced_heavy_tank" }
			if = {
				limit = { is_ai = yes check_variable = { EAI_army_xp > 100 } }
				hidden_effect = { EAI_variant_upgrade_HARM = yes }
			}
		}

		enable_equipments = {
			heavy_tank_equipment_3
		}

		path = {
			leads_to_tech = advanced_heavy_tank_x
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = main_battle_tank
			research_cost_coeff = 1
		}
		path = {
			leads_to_tech = advanced_medium_tank
			research_cost_coeff = 0.9
		}

		path = {
			leads_to_tech = advanced_infantry_tank
			research_cost_coeff = 0.9
		}

		research_cost = 2.5
		start_year = 1943

		folder = {
			name = armour_folder
			position = { x = @heavy_tank y = @1943 }
		}

		sub_technologies = {
			advanced_heavy_td
			advanced_heavy_art
			advanced_heavy_spaa
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1942.6.1 EAI_armor_tech_focus = yes }
			modifier = { factor = 0 date < 1943.1.1 NOT = { EAI_armor_tech_focus = yes } }

			#WEIGHT
			modifier = { factor = 2 EAI_armor_tech_focus = yes }

			#RESTRICTIONS
			modifier = { factor = 0 NOT = { EAI_HARM_tech_focus = yes } }
			modifier = { factor = 0 OR = { has_tech = main_battle_tank is_researching_technology = main_battle_tank } }
		}

		categories = {
			armor
			cat_heavy_armor
		}
	}


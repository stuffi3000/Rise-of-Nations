	advanced_heavy_tank_x = { #E-75

		enable_equipments = {
			heavy_tank_equipment_4
		}

		path = {
			leads_to_tech = main_battle_tank_a
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = main_battle_tank
			research_cost_coeff = 0.9
		}

		sub_technologies = {
			advanced_heavy_td_x
			advanced_heavy_art_x
			advanced_heavy_spaa_x
		}

		research_cost = 2.5
		start_year = 1945

		folder = {
			name = armour_folder
			position = { x = @heavy_tank y = @1945 }
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1944.6.1 EAI_armor_tech_focus = yes }
			modifier = { factor = 0 date < 1945.1.1 NOT = { EAI_armor_tech_focus = yes } }

			#WEIGHT
			modifier = { factor = 2 EAI_armor_tech_focus = yes }

			#RESTRICTIONS
			modifier = { factor = 0 NOT = { EAI_HARM_tech_focus = yes } }
			modifier = { factor = 0 OR = { has_tech = main_battle_tank is_researching_technology = main_battle_tank } }
		}

		categories = {
			armor
			cat_heavy_armor
		}
	}





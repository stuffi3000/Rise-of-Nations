	horses = {

		enable_subunits = {
			cavalry
		}

		on_research_complete_limit = {
			NOT = {
				has_template_containing_unit = cavalry
			}
		}

		on_research_complete = {
			hidden_effect = {
				load_oob = "unlock_cavalry"
			}
			custom_effect_tooltip = UNLOCK_CAVALRY_TEMPLATE
		}

		path = {
			leads_to_tech = ww1_motorised_infantry
			research_cost_coeff = 1
		}

		path = {
			leads_to_tech = RON_Trained_Horses
			research_cost_coeff = 1
		}
		research_cost = 2
		start_year = 1900

		folder = {
			name = armour_folder
			position = { x = @motorized y = @1910 }
		}

		categories = {
			motorized_equipment
		}

		ai_will_do = {
			factor = 1
		}
	}


	radio = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ELECTRONIC: radio" } }

		# units with radios can more efficiently join combats when there is room
		land_reinforce_rate = 0.05

		path = {
			leads_to_tech = radio_detection
			research_cost_coeff = 1
		}

		research_cost = 1.5
		start_year = 1936

		folder = {
			name = electronics_folder
			position = { x = -2 y = @19361 }
		}

		ai_will_do = { #EAI
			factor = 10

			#DATE
			modifier = { factor = 0 date < 1937.1.1 has_war = no }
			modifier = { factor = 10 OR = { date > 1939.1.1 has_war = yes } }
		}

		categories = {
			electronics
		}
	}

	radio_detection = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ELECTRONIC: radio_detection" } }

		# 
		enable_building = {
			building = radar_station
			level = 1
		}

		path = {
			leads_to_tech = decimetric_radar
			research_cost_coeff = 1
		}

		research_cost = 1.5
		start_year = 1936

		folder = {
			name = electronics_folder
			position = { x = -2 y = @19362 }
		}

		ai_will_do = { #EAI
			factor = 1

			#DATE
			modifier = { factor = 0 date < 1942.1.1 NOT = { EAI_radar_tech_focus = yes } }
			modifier = { factor = 0 date < 1940.1.1 EAI_radar_tech_focus = yes }

			#WEIGHT
			modifier = { factor = 20 EAI_radar_tech_focus = yes date > 1939.1.1 }
		}

		categories = {
			electronics
			radar_tech
		}
	}

	decimetric_radar = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ELECTRONIC: decimetric_radar" } }

		# can build radars/radar boost in air/sea combat depends on radar level

		path = {
			leads_to_tech = improved_decimetric_radar
			research_cost_coeff = 1
		}

		research_cost = 1.5
		start_year = 1938
		enable_equipment_modules = {
			ship_radar_1
		}
		folder = {
			name = electronics_folder
			position = { x = -2 y = @1938 }
		}

		ai_will_do = { #EAI
			factor = 1

			#DATE
			modifier = { factor = 0 date < 1942.1.1 NOT = { EAI_radar_tech_focus = yes } }
			modifier = { factor = 0 date < 1938.1.1 EAI_radar_tech_focus = yes }

			#WEIGHT
			modifier = {
				factor = 30
				EAI_radar_tech_focus = yes
				OR = {
					date > 1939.1.1
					AND = {
						date > 1938.1.1
						EAI_radar_tech_focus_2 = yes
					}
				}
			}

			modifier = { factor = 2 EAI_radar_tech_focus_2 = yes }
		}

		categories = {
			electronics
			radar_tech
		}

		enable_building = {
			building = radar_station
			level = 2
		}
	}

	improved_decimetric_radar = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | ELECTRONIC: improved_decimetric_radar" } }

		# radar boost in air/sea combat depends on radar level
		enable_building = {
			building = radar_station
			level = 3
		}

		enable_equipment_modules = {
			ship_radar_2
		}

		path = {
			leads_to_tech = centimetric_radar
			research_cost_coeff = 1
		}

		research_cost = 2
		start_year = 1939

		folder = {
			name = electronics_folder
			position = { x = -2 y = @1939 }
		}

		ai_will_do = { #EAI
			factor = 1

			#DATE
			modifier = { factor = 0 date < 1942.1.1 NOT = { EAI_radar_tech_focus = yes } }
			modifier = { factor = 0 date < 1939.1.1 EAI_radar_tech_focus = yes }

			#WEIGHT
			modifier = { factor = 20 EAI_radar_tech_focus_2 = yes }
		}

		categories = {

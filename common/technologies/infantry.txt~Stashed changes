	### WW2 VANILLA STUFF


	infantry_weapons = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | INFANTRY: infantry_weapons" } }

		enable_equipments = {
			infantry_equipment_0
		}

		path = {
			leads_to_tech = infantry_weapons1
			research_cost_coeff = 1
		}

		research_cost = 1.5
		start_year = 1933
		folder = {
			name = infantry_folder
			position = { x = 0 y = @1933 }
		}

		categories = {
			infantry_weapons
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1936.2.1 }

			#WEIGHT
			modifier = { factor = 3 date > 1936.6.1 OR = { has_war = yes EAI_infantry_tech_focus = yes } }
		}
	}

	infantry_weapons1 = { on_research_complete_limit = { if = { limit = { has_country_flag = EAI_research_logging } log = "[GetDate] | AI TECH | [Root.GetName] | INFANTRY: infantry_weapons1" } }

		enable_equipments = {
			infantry_equipment_1
		}

		path = {
			leads_to_tech = infantry_weapons2
			research_cost_coeff = 1
		}

		research_cost = 1.5
		start_year = 1936
		folder = {
			name = infantry_folder
			position = { x = 0 y = @1936 }
		}

		categories = {
			infantry_weapons
		}

		ai_will_do = { #EAI
			factor = 100

			#DATE
			modifier = { factor = 0 date < 1936.2.1 }

			#WEIGHT

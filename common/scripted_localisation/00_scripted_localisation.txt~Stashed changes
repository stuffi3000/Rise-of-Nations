defined_text = { # for PRC tree
	name = GetManchuriaPuppet
	text = {
		trigger = {
			MAN = {
				OR = {
					is_subject_of = JAP
					has_cosmetic_tag = MAN_kwantung_army
				}
			}
		}
		localization_key = PRC_provoke_japan_manchuria
	}
	text = {
		localization_key = PRC_provoke_japan_korea
	}
}


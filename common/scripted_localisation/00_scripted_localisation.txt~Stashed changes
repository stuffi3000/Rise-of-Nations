defined_text = { # Country
	name = GetGermanTechnologyName
	text = {
		trigger = {
			original_tag = RAJ
			is_in_faction_with = JAP
			NOT = { is_in_faction_with = GER }
		}
		localization_key = RAJ_german_technology_japan
	}
	text = {
		localization_key = RAJ_german_technology_german
	}
}


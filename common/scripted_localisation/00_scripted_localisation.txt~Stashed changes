defined_text = {
	name = GetMilitaristInfluence
	text = {
		##low influence 
		trigger = {
			check_variable = {
				var = mili_influence
				value = 25
				compare = less_than
			}
		}
		localization_key = OTT_low
	}
	text = {
		##medium influence
		trigger = {
			AND = {
				check_variable = {
					var = mili_influence
					value = 50
					compare = greater_than_or_equals
				}
				check_variable = {
					var = mili_influence
					value = 75
					compare = less_than

				}
			}
		}
		localization_key = OTT_medium
	}
	text = {
		##high influence
		trigger = {
			check_variable = {
				var = mili_influence
				value = 75
				compare = greater_than_or_equals
			}

		}
		localization_key = OTT_high
	}
}

defined_text = {
	name = GetStartLeagueOfNationsVotingOnThisCountryTooltip

	text = {
		trigger = {
			has_global_flag = league_of_nations_voting
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_is_a_voting_already
	}
	text = {
		trigger = {
			OR = {
				has_global_flag = GER_anschluss_voting_in_queue
				has_global_flag = SOV_FIN_voting_triggered
			}
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_soon_will_be_a_voting
	}
	text = {
		trigger = {
			OR = {
				has_global_flag = league_of_nations_election
				AND = {
					NOT = { check_variable = { global.days_til_next_league_of_nations_election > 70 } }
					NOT = { has_global_flag = league_of_nations_focus_6_completed }
				}
			}
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_is_an_election_soon
	}
	text = {
		trigger = {
			check_variable = { cooldown_for_league_of_nations_voting > 0 }
		}
		localization_key = cannot_start_league_of_nations_voting_because_recently_there_was_a_voting_for_this_country
	}
	text = {
		trigger = {
			check_variable = { global.time_til_next_possible_league_of_nations_voting > 0 }
		}
		localization_key = cannot_start_league_of_nations_voting_because_recently_there_was_a_voting
	}
	text = {
		trigger = {
			NOT = { check_variable = { num_of_active_league_of_nations_trade_sanctions < 10 } }
		}
		localization_key = cannot_start_league_of_nations_voting_because_too_many_trade_sanctions
	}
	text = {
		trigger = {
			has_country_flag = leader_of_league_of_nations
		}
		localization_key = cannot_start_league_of_nations_voting_because_target_is_leader_of_league_of_nations
	}
	text = {
		trigger = {
			check_variable = { league_of_nations_generated_threat < 10 }
		}
		localization_key = cannot_start_league_of_nations_voting_because_target_has_not_created_enough_wt
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = start_league_of_nations_voting_tt
	}
}

defined_text = {
	name = GetStartLeagueOfNationsVotingTooltip

	text = {
		trigger = {
			has_global_flag = league_of_nations_voting
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_is_a_voting_already
	}
	text = {
		trigger = {
			OR = {
				has_global_flag = GER_anschluss_voting_in_queue
				has_global_flag = SOV_FIN_voting_triggered
			}
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_soon_will_be_a_voting
	}
	text = {
		trigger = {
			OR = {
				has_global_flag = league_of_nations_election
				AND = {
					NOT = { check_variable = { global.days_til_next_league_of_nations_election > 70 } }
					NOT = { has_global_flag = league_of_nations_focus_6_completed }
				}
			}
		}
		localization_key = cannot_start_league_of_nations_voting_because_there_is_an_election_soon
	}
	text = {
		trigger = {
			check_variable = { global.time_til_next_possible_league_of_nations_voting > 0 }
		}
		localization_key = cannot_start_league_of_nations_voting_because_recently_there_was_a_voting
	}
	text = {
		trigger = {
			NOT = {
				any_other_country = {
					NOT = { has_country_flag = leader_of_league_of_nations }
					check_variable = { num_of_active_league_of_nations_trade_sanctions < 10 }
					check_variable = { cooldown_for_league_of_nations_voting < 1 }
					NOT = { check_variable = { league_of_nations_generated_threat < 10 } }
				}
			}
		}
		localization_key = cannot_start_league_of_nations_voting_because_no_possible_target

defined_text = {
	name = GetTimeTilNextLeagueOfNationsTradeSanctionExpires

	text = {
		trigger = {
			check_variable = { duration_of_10_league_of_nations_trade_sanction > 0 }
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_1_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_1_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_2_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_2_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_3_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_3_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_4_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_4_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_5_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_5_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_6_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_6_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_7_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_7_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_8_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_8_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_10_league_of_nations_trade_sanction > duration_of_9_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > 0 } }
			}
		}
		localization_key = string_time_til_league_of_nations_trade_sanction_expires_10
	}
	text = {
		trigger = {
			check_variable = { duration_of_9_league_of_nations_trade_sanction > 0 }
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_1_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_1_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_2_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_2_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_3_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_3_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_4_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_4_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_5_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_5_league_of_nations_trade_sanction > 0 } }
			}
			OR = {
				NOT = { check_variable = { duration_of_9_league_of_nations_trade_sanction > duration_of_6_league_of_nations_trade_sanction } }
				NOT = { check_variable = { duration_of_6_league_of_nations_trade_sanction > 0 } }
			}
			OR = {

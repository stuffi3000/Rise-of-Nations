defined_text = {
	name = GetLeagueOfNationsSelectedFocusStartTooltip

	text = {
		trigger = {
			has_global_flag = league_of_nations_focus_active
		}
		localization_key = league_of_nations_focus_cannot_start_because_of_there_is_a_focus_active
	}
	text = {
		trigger = {
			OR = {
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 1 }
					has_global_flag = league_of_nations_focus_1_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 2 }
					has_global_flag = league_of_nations_focus_2_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 3 }
					has_global_flag = league_of_nations_focus_3_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 4 }
					has_global_flag = league_of_nations_focus_4_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 5 }
					has_global_flag = league_of_nations_focus_5_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 6 }
					has_global_flag = league_of_nations_focus_6_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 7 }
					has_global_flag = league_of_nations_focus_7_completed
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 8 }
					has_global_flag = league_of_nations_focus_8_completed
				}
			}
		}
		localization_key = league_of_nations_focus_cannot_start_because_this_focus_already_is_completed
	}
	text = {
		trigger = {
			OR = {
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 1 }
					always = no # No requirements til now
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 2 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
						has_global_flag = league_of_nations_focus_3_selected
						has_global_flag = league_of_nations_focus_3_completed
						NOT = { check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 } }
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 3 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
						has_global_flag = league_of_nations_focus_2_selected
						has_global_flag = league_of_nations_focus_2_completed
						NOT = { check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 } }
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 4 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_2_completed }
						NOT = { date > 1939.09.17 }
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 5 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_3_completed }
						NOT = { date > 1939.09.17 }
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 6 }
					OR = {
						AND = {
							NOT = { has_global_flag = league_of_nations_focus_4_completed }
							NOT = { has_global_flag = league_of_nations_focus_5_completed }
						}
						NOT = { date > 1942.09.17 }
						NOT = { check_variable = { num_of_won_league_of_nations_elections > 2 } }
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 7 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_4_completed }
						NOT = { has_global_flag = league_of_nations_focus_6_completed }
						NOT = { date > 1945.09.17 }
						has_war = yes
					}
				}
				AND = {
					check_variable = { show_selected_league_of_nations_focus = 8 }
					OR = {
						NOT = { has_global_flag = league_of_nations_focus_5_completed }
						NOT = { has_global_flag = league_of_nations_focus_6_completed }
						NOT = { date > 1945.09.17 }
						has_war = yes
					}
				}
			}
		}
		localization_key = league_of_nations_focus_cannot_start_because_requirements_are_not_met
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = league_of_nations_focus_can_start
	}
}


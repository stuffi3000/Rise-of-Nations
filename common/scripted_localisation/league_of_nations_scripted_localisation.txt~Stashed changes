# Threat
defined_text = {
	name = GetLeagueOfNationsThreatRate

	text = {
		trigger = {
			check_variable = { league_of_nations_generated_threat_rate > 0 }
		}
		localization_key = string_league_of_nations_threat_rate_red
	}
	text = {
		trigger = {
			check_variable = { league_of_nations_generated_threat_rate < 0 }
		}
		localization_key = string_league_of_nations_threat_rate_green
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = string_league_of_nations_threat_rate_yellow
	}
}

defined_text = {
	name = GetLeagueOfNationsThreatRateReasons

	text = {
		trigger = {
			always = yes
		}
		localization_key = string_league_of_nations_threat_rate_reasons
	}
}

defined_text = {
	name = GetLeagueOfNationsThreatRateReasonWarPeace

	text = {
		trigger = {
			check_variable = { league_of_nations_threat_from_war_peace = -0.4 }
		}
		localization_key = string_league_of_nations_threat_rate_reason_peace
	}
	text = {
		trigger = {
			check_variable = { league_of_nations_threat_from_war_peace = -0.12 }
		}
		localization_key = string_league_of_nations_threat_rate_reason_defensive_war
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = string_league_of_nations_threat_rate_reason_nothing
	}
}

defined_text = {
	name = GetLeagueOfNationsThreatRateReasonOffensiveWar

	text = {
		trigger = {
			check_variable = { league_of_nations_threat_from_offensive_war = 0.2 }
		}
		localization_key = string_league_of_nations_threat_rate_reason_war_against_member
	}
	text = {
		trigger = {
			OR = {
				check_variable = { league_of_nations_threat_from_offensive_war = 0.3 }
				check_variable = { league_of_nations_threat_from_offensive_war = 0.5 }
			}
		}
		localization_key = string_league_of_nations_threat_rate_reason_volunteers
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = string_league_of_nations_threat_rate_reason_nothing
	}
}

defined_text = {
	name = GetIfThereAreLeagueOfNationsThreatRateReasons

	text = {
		trigger = {
			NOT = { check_variable = { league_of_nations_threat_from_war_peace = -0.4 } }
			NOT = { check_variable = { league_of_nations_threat_from_war_peace = -0.12 } }
			NOT = { check_variable = { league_of_nations_threat_from_offensive_war = 0.2 } }
			NOT = { check_variable = { league_of_nations_threat_from_offensive_war = 0.3 } }
			NOT = { check_variable = { league_of_nations_threat_from_offensive_war = 0.5 } }
			check_variable = { league_of_nations_threat_from_war_peace = 0 }
		}
		localization_key = string_league_of_nations_threat_rate_no_reasons
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = string_league_of_nations_threat_rate_reasons_exist
	}
}


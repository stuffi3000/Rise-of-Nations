defined_text = { ###
	name = CHI_foreign_investments_cat_desc_SL
	text = {
		trigger = {
			tag = CHI
		}
		localization_key = CHI_CHI_foreign_investments_cat_desc
	}
	text = {
		trigger = {
			OR = {
				tag = ENG
				tag = FRA
				tag = GER
				tag = ITA
				tag = SOV
				tag = JAP
				tag = USA

				tag = TUR
				tag = BRA
				tag = AST
			}
		}
		localization_key = CHI_INV_foreign_investments_cat_desc
	}
}




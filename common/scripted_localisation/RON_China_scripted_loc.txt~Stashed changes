	name = GetAllCellLocationAndStrength
	text = {
		trigger = {
			any_state = {
				has_state_flag = KMT_PRC_state_has_guerrilla_cell
				is_core_of = ROOT
			}
		}
		localization_key = PRC_get_all_location_and_cell_strength
	}
	text = {
		localization_key = ""
	}
}


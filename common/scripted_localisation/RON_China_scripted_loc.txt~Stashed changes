defined_text = {
	### Based on NAT. SPIRIT it will show the LVL of PS
	name = SHA_popular_support_sl

	### RECOGNIZED
	text = {
		trigger = {
			tag = CHI
			SHA_is_recognized_government_of_china = yes
		}
		localization_key = CHI_ps_recognized_EXPL
	}
	text = {
		trigger = {
			tag = PRC
			SHA_is_recognized_government_of_china = yes
		}
		localization_key = PRC_ps_recognized_EXPL
	}
	text = {
		trigger = {
			tag = MAN
			SHA_is_recognized_government_of_china = yes
		}
		localization_key = MAN_ps_recognized_EXPL
	}

	### UNRECOGNIZED
	text = {
		trigger = {
			tag = CHI
			SHA_is_recognized_government_of_china = no
		}
		localization_key = CHI_ps_not_yet_recognized_EXPL
	}
	text = {
		trigger = {
			tag = PRC
			SHA_is_recognized_government_of_china = no
		}
		localization_key = PRC_ps_not_yet_recognized_EXPL
	}
	text = {
		trigger = {
			tag = MAN
			SHA_is_recognized_government_of_china = no
		}
		localization_key = MAN_ps_not_yet_recognized_EXPL
	}

	### NOT CONTROLLING TERRITORY
	text = {
		trigger = {
			tag = CHI
			exists = no
		}
		localization_key = CHI_ps_no_territory_EXPL
	}
	text = {
		trigger = {
			tag = PRC
			exists = no
		}
		localization_key = PRC_ps_no_territory_EXPL
	}
	text = {
		trigger = {
			tag = MAN
			exists = no
		}
		localization_key = MAN_ps_no_territory_EXPL
	}
}



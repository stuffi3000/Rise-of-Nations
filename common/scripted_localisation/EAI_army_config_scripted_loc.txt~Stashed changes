defined_text = {
	name = EAI_GetInfantryDesign

	text = {
		trigger = { var:EAI_configuration_target = { OR = { AND = { check_variable = { EAI_FOCUS/LAND/infantry_design = global.EAI_A_INF_DESIGN } NOT = { has_variable = EAI_FOCUS/LAND/infantry_design_OVERRIDE } } check_variable = { EAI_FOCUS/LAND/infantry_design_OVERRIDE = global.EAI_A_INF_DESIGN } } } }
		localization_key = EAI_config_ARMY_L2a_text
	}
	text = {
		trigger = { var:EAI_configuration_target = { OR = { AND = { check_variable = { EAI_FOCUS/LAND/infantry_design = global.EAI_B_INF_DESIGN } NOT = { has_variable = EAI_FOCUS/LAND/infantry_design_OVERRIDE } } check_variable = { EAI_FOCUS/LAND/infantry_design_OVERRIDE = global.EAI_B_INF_DESIGN } } } }
		localization_key = EAI_config_ARMY_L2b_text
	}
	text = {
		trigger = { var:EAI_configuration_target = { OR = { AND = { check_variable = { EAI_FOCUS/LAND/infantry_design = global.EAI_C_INF_DESIGN } NOT = { has_variable = EAI_FOCUS/LAND/infantry_design_OVERRIDE } } check_variable = { EAI_FOCUS/LAND/infantry_design_OVERRIDE = global.EAI_C_INF_DESIGN } } } }
		localization_key = EAI_config_ARMY_L2c_text
	}

	text = {
		trigger = { var:EAI_configuration_target = { NOT = { has_variable = EAI_FOCUS/LAND/infantry_design has_variable = EAI_FOCUS/LAND/infantry_design_OVERRIDE } } }
		localization_key = EAI_config_ARMY_L2-_text
	}
}


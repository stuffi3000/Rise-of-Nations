defined_text = {
	name = GetTradeSanctionsOkay

	text = {
		trigger = {
			has_global_flag = UNSC_sanction_voting_ongoing
		}
		localization_key = UNSC_sanctions_ongoing
	}
	text = {
		trigger = {
			NOT = { has_global_flag = UNSC_sanction_voting_ongoing }
		}
		localization_key = UNSC_trade_sanctions_loc
	}
}


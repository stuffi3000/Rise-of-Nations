defined_text = {
	name = GetSanctionType

	text = {
		trigger = {
			OR = {
				has_global_flag = UNSC_trade_sanctions
				ROOT = {
					has_country_flag = apply_for_trade_sanctions
				}
			}
		}
		localization_key = UNSC_trade_sanctions_type
	}
	text = {
		trigger = {
			OR = {
				has_global_flag = UNSC_security_risk
				ROOT = {
					has_country_flag = apply_for_security_risk
				}
			}
		}
		localization_key = UNSC_security_risk_type
	}
}

defined_text = {
	name = GetMemberListEntryBGColor

	text = {
		trigger = {
			THIS = {
				has_country_flag = united_nations_non_permanent_unsc_seat
			}
		}
		localization_key = GFX_league_of_nations_member_list_entry_grey
	}
	text = {
		trigger = {
			THIS = {
				has_country_flag = united_nations_permanent_unsc_seat
			}
		}
		localization_key = GFX_league_of_nations_member_list_entry
	}
}

defined_text = {
	name = GetMemberVoteStatus

	text = {
		trigger = {
			THIS = {
				NOT = { has_global_flag = UNSC_sanction_voting_ongoing }
			}
		}
		localization_key = GFX_UNSC_button_bg
	}
	text = {
		trigger = {
			THIS = {
				has_global_flag = UNSC_sanction_voting_ongoing
				has_country_flag = UNSC_voted_for
			}
		}
		localization_key = GFX_UNSC_vote_for
	}
	text = {
		trigger = {
			THIS = {
				has_global_flag = UNSC_sanction_voting_ongoing
				has_country_flag = UNSC_voted_against
			}
		}
		localization_key = GFX_UNSC_vote_against
	}
	text = {
		trigger = {
			THIS = {
				has_global_flag = UNSC_sanction_voting_ongoing
				has_country_flag = UNSC_voted_abstain
			}
		}
		localization_key = GFX_UNSC_vote_abstain
	}
	text = {
		trigger = {
			THIS = {
				has_global_flag = UNSC_sanction_voting_ongoing
				has_country_flag = UNSC_voted_veto
			}
		}
		localization_key = GFX_UNSC_vote_veto
	}
}

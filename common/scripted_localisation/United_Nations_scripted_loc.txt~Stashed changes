defined_text = {
	name = GetUNSCListEntry1

	text = {
		trigger = {
			any_country = {
				has_country_flag = permanent_unsc_member_1
				exists = yes
			}
		}
		localization_key = unsc_list_entry_1
	}
	text = {
		trigger = {
			always = yes
		}
		localization_key = unsc_member_no_entry
	}
}


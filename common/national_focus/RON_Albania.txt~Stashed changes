focus_tree = {
	id = RON_Albania

	country = {
		factor = 0
		modifier = {
			add = 20
			tag = ALB
		}
	}
	continuous_focus_position = {
		x = 200
		y = 3000
	}
	default = no

	###############################################################################################################################

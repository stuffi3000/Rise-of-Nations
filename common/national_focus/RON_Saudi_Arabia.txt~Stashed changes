focus_tree = {
	id = RON_Saudi_Arabia

	country = {
		factor = 0

		modifier = {
			add = 10
			tag = SAU
		}
	}

	default = no
	continuous_focus_position = {
		x = 50
		y = 1700
	}

	###############################################################################################################################

	x = 22
	y = 0
	cost = 8

	allow_branch = {
		date < 1910.1.1
	}

	ai_will_do = { factor = 10 }

	completion_reward = {
		add_tech_bonus = {
			bonus = 0.5
			uses = 1
			category = industry
		}
	}
}

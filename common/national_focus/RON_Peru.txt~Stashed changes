focus_tree = {
	id = RON_Peru

	initial_show_position = {
		focus = PRU_politics
	}

	country = {
		factor = 0

		modifier = {
			add = 10
			tag = PRU
		}

	}

	default = no

	continuous_focus_position = { x = 50 y = 1700 }

	###############################################################################################################################

			factor = 10
		}
		completion_reward = {
			add_timed_idea = {
				idea = RI_economic_recovery
				days = 1095
			}
		}
		prerequisite = { focus = PRU1933_strenghten_the_government }
	}

	#Strenghten the Government
	focus = {
		id = PRU1933_strenghten_the_government

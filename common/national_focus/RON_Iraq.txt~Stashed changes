focus_tree = {
	id = RON_Iraq

	country = {
		factor = 0

		modifier = {
			add = 10
			tag = IRQ
		}
	}

	default = no
	continuous_focus_position = {
		x = 50
		y = 1700
	}

	###############################################################################################################################

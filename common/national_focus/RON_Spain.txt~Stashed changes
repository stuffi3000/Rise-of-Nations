focus_tree = {
	id = RON_Spain
	country = {
		factor = 0

		modifier = {
			add = 10
			original_tag = SPR
		}
	}

	default = no
	reset_on_civilwar = no

	continuous_focus_position = {
		x = 200
		y = 3000
	}

	###############################################################################################################################

			factor = 1
		}
		completion_reward = {
			SPR = {
				add_ideas = { SPA_carlist_groups }
			}
			every_country = {
				limit = {
					original_tag = SPR
					has_government = neutrality
				}
				country_event = { id = generic.5 }
			}
		}
	}


	focus = {
		id = FRA_vienot_agreement_
		icon = GFX_goal_generic_improve_relations
		prerequisite = { focus = FRA_colonial_investments }
		mutually_exclusive = { focus = FRA_continue_mandat }
		x = 5
		y = 1
		relative_position_id = FRA_colonial_investments
		cost = 10

		available = {
			is_subject = no
		}
		bypass = {
			NOT = {
				AND = {
					SYR = { is_subject_of = FRA }
					LEB = { is_subject_of = FRA }
				}
			}
		}

		available_if_capitulated = yes

		ai_will_do = {
			factor = 100
		}
		completion_reward = {
			add_stability = -0.05
			if = {
				limit = {
					SYR = {
						is_subject_of = FRA
					}
				}
				release = SYR
				set_autonomy = { target = SYR autonomy_state = autonomy_free }
				give_guarantee = SYR
				SYR = { add_opinion_modifier = { target = FRA modifier = ENG_guiding_the_colonies } }
				hidden_effect = {

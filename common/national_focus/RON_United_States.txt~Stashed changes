	focus = {
		id = USA1_build_railroads
		icon = GFX_goal_generic_construct_infrastructure
		x = 34
		y = 4
		cost = 10
		prerequisite = { focus = USA1_fiscal_reforms
			focus = USA1_loosen_restrictions }
		prerequisite = { focus = USA1_full_speed_ahead }
		completion_reward = {
			random_owned_controlled_state = {
				limit = {
					free_building_slots = {
						building = infrastructure
						size > 0
					}
					OR = {
						is_in_home_area = yes
						NOT = {
							owner = {
								any_owned_state = {
									free_building_slots = {
										building = infrastructure
										size > 0
									}
									is_in_home_area = yes
								}
							}
						}
					}
				}
				add_building_construction = {
					type = infrastructure
					level = 1
					instant_build = yes
				}
			}
			random_owned_controlled_state = {
				limit = {
					free_building_slots = {
						building = infrastructure
						size > 1
					}
					OR = {

	focus = {
		id = USA_Liberal_III
		icon = GFX_focus_chi_mission_to_the_us
		mutually_exclusive = { focus = USA_Leftist_III }
		prerequisite = { focus = USA_Servicemens_Readjustment_Act }
		prerequisite = { focus = USA_Womens_Armed_Forces_Integration_Act }
		x = 17
		y = 14
		cost = 2
		available = {
			has_government = democratic
			has_completed_focus = USA_Demobilization
			OR = {
				has_completed_focus = USA_Liberal_I
				has_completed_focus = USA_Liberal_II
			}
			NOT = { has_country_leader = { id = 1776 ruling_only = yes name = "Franklin Delano Roosevelt" } }
			NOT = {
				has_war = yes
				ENG = { has_war = yes }
				FRA = { has_war = yes }
				SOV = { has_war = yes }
				GER = { has_war = yes }
				ITA = { has_war = yes }
				JAP = { has_war = yes }
				CHI = { has_war = yes }
			}
			NOT = { has_completed_focus = USA_Permanent_War }
		}
		ai_will_do = {
			factor = 10
		}
		completion_reward = {
			#United Nations Participation Act
			set_rule = { can_boost_other_ideologies = yes }
			if = {
				limit = { is_in_faction = yes }
				leave_faction = yes
			}
			create_faction = "The United Nations"
			every_other_country = {
				limit = { is_major = yes has_war = no }
				country_event = { id = usa.218 days = 1 }
			}
		}

	}



							owns_state = 676
							owns_state = 423
							owns_state = 431
						}
					}
				}
				create_wargoal = {
					type = take_state_focus
					target = RAJ
					generator = { 676 423 431 }
				}
			}
			if = {
				limit = {
					SIA = {
						OR = {
							owns_state = 289
							owns_state = 724
						}
					}
				}
				create_wargoal = {
					type = take_state_focus
					target = SIA
					generator = { 289 724 }
				}
			}
			ENG = {
				add_opinion_modifier = { target = AST modifier = AST_asian_expansionism }
			}
			SOV = {
				add_opinion_modifier = { target = AST modifier = AST_asian_expansionism }

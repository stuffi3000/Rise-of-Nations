	focus = {
		id = AUH1_dockyards
		icon = GFX_goal_generic_construct_naval_dockyard
		x = 66
		y = 2
		cost = 10
		prerequisite = { focus = AUH1_battlecruisers
			focus = AUH1_patrol_boats
			focus = AUH1_new_cruisers
			focus = AUH1_grandest_ships }

focus_tree = {
	id = RON_Yugoslavia

	country = {
		factor = 0

		modifier = {
			add = 10
			tag = YUG
		}
	}

	default = no

	continuous_focus_position = { x = 100 y = 2250 }

	###############################################################################################################################

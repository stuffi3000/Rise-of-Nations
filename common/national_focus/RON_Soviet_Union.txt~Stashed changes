	#Focus for Second Five Year plan
	focus = {
		id = SOV_1933_second_five_year_plan
		icon = GFX_goal_generic_construction2
		x = 6
		y = 0
		cost = 10
		available_if_capitulated = yes
		ai_will_do = {
			factor = 1
		}
		allow_branch = {
			date > 1932.12.31
			date < 1949.12.31
		}
		completion_reward = {
			add_timed_idea = { idea = RI_five_year_plan days = 1800 }
			remove_ideas = { RI_commie_unions }
		}
	}


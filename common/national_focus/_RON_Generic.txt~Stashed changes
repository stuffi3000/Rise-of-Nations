	## Cold War Axis Victory Diplomacy TREE


	focus = {
		id = axvic_diplomatic_focus
		icon = GFX_focus_ron_earth_icon
		x = 23
		y = 15
		cost = 1

		allow_branch = {
			date > 1948.12.31
			date < 1980.1.1
			has_global_flag = Greater_Germanic_Reich_Formed
		}

		available = {
			has_capitulated = no
		}

		completion_reward = {
			add_political_power = 5
		}
	}

	focus = {
		id = axvic_new_world_order
		icon = GFX_focus_spa_strengthen_the_supreme_reality_of_spain
		x = 7
		y = 16
		cost = 1

		available = {
			is_puppet = no
			OR = {
				has_government = fascism
				has_government = neutrality
			}
			OR = {
				JAP = {
					has_government = fascism
				}
				ITA = {
					has_government = fascism
				}
				GER = {
					has_government = fascism
				}
			}
		}

		prerequisite = { focus = axvic_diplomatic_focus }

		mutually_exclusive = { focus = axvic_resist_fascists }
		mutually_exclusive = { focus = axvic_stay_neutral }

		completion_reward = {
			add_political_power = 5
		}
	}


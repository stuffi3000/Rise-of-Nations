focus_tree = {
	id = generic_focus
	country = {
		factor = 1
	}
	default = yes
	reset_on_civilwar = no
	continuous_focus_position = {
		x = 200
		y = 3000
	}

	###############################################################################################################################
	###############################################################################################################################

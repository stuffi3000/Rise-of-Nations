	focus = {
		id = DNC_open_political_sphere
		icon = GFX_goal_generic_major_alliance
		x = 15
		y = 0
		cost = 10
		allow_branch = {
			date < 1910.1.1
		}
		available = {
			is_subject = no
		}
		ai_will_do = {
			factor = 10
			modifier = {
				factor = 0
				date < 1905.1.1
			}
		}
		completion_reward = {
			add_political_power = 150
		}
	}
	focus = {
		id = DNC_communist_path
		icon = GFX_goal_support_communism
		x = 3
		y = 1
		cost = 10
		prerequisite = {
			focus = DNC_open_political_sphere
		}
		mutually_exclusive = {
			focus = DNC_non_alligned_path
		}
		mutually_exclusive = {
			focus = DNC_fascist_path
		}
		mutually_exclusive = {
			focus = DNC_democratic_path
		}
		mutually_exclusive = {
			focus = DNC_islamist_path
		}
		ai_will_do = {
			factor = 10
			modifier = {
				#EAI
				factor = 0
				is_historical_focus_on = yes
				NOT = {
					has_government = communism
				}
			}
		}
		completion_reward = {
			add_timed_idea = {
				idea = RI_commies_rise
				days = 1095
			}
		}
	}

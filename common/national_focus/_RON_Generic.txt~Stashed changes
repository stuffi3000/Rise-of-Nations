	focus = {
		id = axvictwo_acquire_weapons
		icon = GFX_focus_generic_combined_arms
		x = 34
		y = 18
		cost = 3
		prerequisite = { focus = axvictwo_prepare_for_war }
		complete_effect = {
			random_owned_controlled_state = {
				limit = {
					free_building_slots = {
						building = arms_factory
						size > 0
						include_locked = yes
					}
					OR = {
						is_in_home_area = yes
						noT = {
							owner = {
								any_owned_state = {
									free_building_slots = {
										building = arms_factory
										size > 0
										include_locked = yes
									}
									is_in_home_area = yes
								}
							}
						}
					}
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = arms_factory
					level = 1
					instant_build = yes
				}
			}
			add_ideas = { IR_more_weapons }
		}
	}


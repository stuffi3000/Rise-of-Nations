focus_tree = {
	id = RON_Cuba
	country = {
		factor = 0
		modifier = {
			add = 10
			tag = CUB
		}
	}
	default = no
	continuous_focus_position = {
		x = 200
		y = 3000
	}

	###############################################################################################################################

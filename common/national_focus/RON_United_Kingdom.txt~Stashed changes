			custom_effect_tooltip = available_political_advisor
			show_ideas_tooltip = ENG_winston_churchill
			custom_effect_tooltip = available_political_advisor
			show_ideas_tooltip = ENG_oswald_mosley
			custom_effect_tooltip = available_political_advisor
			show_ideas_tooltip = ENG_david_lloyd_george
			hidden_effect = {
				add_ai_strategy = {
					type = contain

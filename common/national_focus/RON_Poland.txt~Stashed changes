	available = {
		is_faction_leader = yes
	}

	bypass = {
		OR = {
			LIT = {
				OR = {
					is_subject = yes
					exists = no
					AND = {
						is_in_faction = yes
						NOT = { is_in_faction_with = POL }
					}
				}
			}
			POL = {
				owns_state = 188
				owns_state = 189
				owns_state = 11
			}
		}
	}


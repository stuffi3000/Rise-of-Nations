	id = POL_INT_return_danzig_to_germany
	icon = GFX_goal_generic_dangerous_deal
	x = 65
	y = 14
	cost = 10
	available_if_capitulated = yes
	prerequisite = { focus = POL_INT_obey_the_treaty }
	mutually_exclusive = { focus = POL_INT_subdue_danzig }
	ai_will_do = {
		factor = 1
	}
	completion_reward = {
		add_opinion_modifier = {
			target = GER
			modifier = medium_increase
		}
		remove_state_core = 85
	}
}


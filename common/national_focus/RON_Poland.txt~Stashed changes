focus = {
	id = POL_develop_ukraine
	icon = GFX_goal_generic_construct_infrastructure
	prerequisite = { focus = POL_reorganize_ukraine }

	available = {
		OR = {
			any_owned_state = {
				is_core_of = UKR
			}
			UKR = {
				exists = yes
				is_subject_of = POL
			}
		}
	}

	#bypass = {
	#
	#}

	x = -1
	y = 1
	relative_position_id = POL_reorganize_ukraine
	cost = 10

	ai_will_do = {
		factor = 10
	}

	completion_reward = {
		every_state = {
			limit = {
				OR = {
					AND = {
						is_core_of = UKR
						is_owned_and_controlled_by = POL
					}
					AND = {
						UKR = { is_subject_of = POL }
						is_owned_and_controlled_by = UKR
					}
				}
			}
			if = {
				limit = {
					infrastructure < 6
				}
				set_building_level = {
					type = infrastructure
					level = 6
					instant_build = yes
				}
				add_extra_state_shared_building_slots = 1
			}
			custom_effect_tooltip = set_infrastructure_to_6
		}
	}
}


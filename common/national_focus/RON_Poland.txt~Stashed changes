#Dockyard focus
focus = {
	id = POL1933_dockyard_focus
	icon = GFX_goal_generic_construct_naval_dockyard
	x = 32
	y = 5
	relative_position_id = POL1933_the_will_of_the_people
	cost = 10
	search_filters = {
		FOCUS_FILTER_INDUSTRY
	}
	ai_will_do = {
		factor = 10
	}
	prerequisite = { focus = POL1933_raider_fleet }
	prerequisite = { focus = POL1933_fleet_in_being }
	completion_reward = {
		if = {
			limit = {
				any_state = {
					is_coastal = yes
					is_controlled_by = ROOT
				}
			}
			random_owned_state = {
				limit = {
					free_building_slots = {
						building = dockyard
						size > 0
						include_locked = yes
					}
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = dockyard
					level = 1
					instant_build = yes
				}
			}
			random_owned_state = {
				limit = {
					free_building_slots = {
						building = dockyard
						size > 0
						include_locked = yes
					}
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = dockyard
					level = 1
					instant_build = yes
				}
			}
		}
	}
}


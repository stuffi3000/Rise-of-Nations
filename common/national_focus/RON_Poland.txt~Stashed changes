focus = {
	id = POL_integrate_eastern_lands
	icon = GFX_focus_generic_diplomatic_treaty
	prerequisite = { focus = POL_ukrainian_industry }
	prerequisite = { focus = POL_belarusian_industry }
	prerequisite = { focus = POL_develop_ukraine }
	prerequisite = { focus = POL_develop_belarus }

	available = {
		OR = {
			any_owned_state = {
				OR = {
					is_core_of = BLR
					is_core_of = UKR
				}
				NOT = { is_core_of = POL }
			}
			UKR = {
				exists = yes
				is_subject_of = POL
			}
			BLR = {
				exists = yes
				is_subject_of = POL
			}
		}
	}

	#bypass = {
	#
	#}

	x = 1
	y = 1
	relative_position_id = POL_ukrainian_industry
	cost = 10

	ai_will_do = {
		factor = 10
	}

	completion_reward = {
		every_state = {
			limit = {
				OR = {
					is_core_of = BLR
					is_core_of = UKR
				}
				OR = {
					is_owned_and_controlled_by = POL
					AND = {
						is_owned_and_controlled_by = UKR
						UKR = { is_subject_of = POL }
					}
					AND = {
						is_owned_and_controlled_by = BLR
						BLR = { is_subject_of = POL }
					}
				}
			}
			add_core_of = POL
		}
		if = {
			limit = {
				UKR = {
					exists = yes
					is_subject_of = POL
				}
			}
			annex_country = {
				target = UKR
				transfer_troops = yes
			}
		}
		if = {
			limit = {
				BLR = {
					exists = yes
					is_subject_of = POL
				}
			}
			annex_country = {
				target = BLR
				transfer_troops = yes
			}
		}
	}
}

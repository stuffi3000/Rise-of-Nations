#March to the *Capital*
focus = {
	id = POL1933_march_to_the_capital
	icon = GFX_focus_eng_move_to_secure_the_dominions
	x = 7
	y = 2
	relative_position_id = POL1933_the_will_of_the_people
	cost = 5
	search_filters = {
		FOCUS_FILTER_POLITICAL
	}
	ai_will_do = {
		factor = 10
	}
	available = {
		fascism > 0.50
	}
	completion_reward = {

		if = {
			limit = {
				has_government = fascism
			}
			add_political_power = 150
		}

		if = {
			limit = {
				NOT = { has_government = fascism }
			}

			if = {
				limit = {
					fascism < 0.60
				}
				random_list = {
					50 = {
						start_civil_war = {
							ruling_party = fascism
							ideology = ROOT
							size = 0.5
						}
					}
					50 = {
						country_event = {
							id = ww2tree.10
							days = 1
						}
					}
				}
			}

			if = {
				limit = {
					fascism > 0.59
					fascism < 0.70
				}
				random_list = {
					40 = {
						start_civil_war = {
							ruling_party = fascism
							ideology = ROOT
							size = 0.4
						}
					}
					60 = {
						country_event = {
							id = ww2tree.10
							days = 1
						}
					}
				}
			}

			if = {
				limit = {
					fascism > 0.69
					fascism < 0.80
				}
				random_list = {
					30 = {
						start_civil_war = {
							ruling_party = fascism
							ideology = ROOT
							size = 0.3
						}
					}
					70 = {
						country_event = {
							id = ww2tree.10
							days = 1
						}
					}
				}
			}

			if = {
				limit = {
					fascism > 0.79
					fascism < 0.90
				}
				random_list = {
					20 = {
						start_civil_war = {
							ruling_party = fascism
							ideology = ROOT
							size = 0.2
						}
					}
					80 = {
						country_event = {
							id = ww2tree.10
							days = 1
						}
					}
				}
			}

			if = {
				limit = {
					fascism > 0.89
				}
				random_list = {
					10 = {
						start_civil_war = {
							ruling_party = fascism
							ideology = ROOT
							size = 0.1
						}
					}
					90 = {
						country_event = {
							id = ww2tree.10
							days = 1
						}
					}
				}
			}
		}
	}
	prerequisite = { focus = POL1933_fascism_leads_the_way }
	prerequisite = { focus = POL1933_create_militias }
}

#Lenin's Legacy
focus = {
	id = POL1933_lenins_legacy
	icon = GFX_focus_spr_class_war
	x = 0
	y = 2
	relative_position_id = POL1933_the_will_of_the_people
	cost = 5
	search_filters = {
		FOCUS_FILTER_POLITICAL
	}
	ai_will_do = {
		factor = 10
	}
	available = {
		communism > 0.50
	}
	completion_reward = {
		if = {
			limit = {
				has_government = communism
			}
			add_political_power = 150
		}

		if = {
			limit = {
				NOT = { has_government = communism }
			}
			if = {
				limit = {
					communism < 0.60
				}
				random_list = {
					50 = {
						start_civil_war = {
							ruling_party = communism
							ideology = ROOT
							size = 0.5
						}
					}
					50 = {
						country_event = {
							id = ww2tree.3
							days = 1
						}
					}
				}
			}
			if = {
				limit = {
					communism > 0.59
					communism < 0.70
				}
				random_list = {
					40 = {
						start_civil_war = {
							ruling_party = communism
							ideology = ROOT
							size = 0.4
						}
					}
					60 = {
						country_event = {
							id = ww2tree.3
							days = 1
						}
					}
				}
			}
			if = {
				limit = {
					communism > 0.69
					communism < 0.80
				}
				random_list = {
					30 = {
						start_civil_war = {
							ruling_party = communism
							ideology = ROOT
							size = 0.3
						}
					}
					70 = {
						country_event = {
							id = ww2tree.3
							days = 1
						}
					}
				}
			}
			if = {
				limit = {
					communism > 0.79
					communism < 0.90
				}
				random_list = {
					20 = {
						start_civil_war = {
							ruling_party = communism
							ideology = ROOT
							size = 0.2
						}
					}
					80 = {
						country_event = {
							id = ww2tree.3
							days = 1
						}
					}
				}
			}

			if = {
				limit = {
					communism > 0.89
				}
				random_list = {
					10 = {
						start_civil_war = {
							ruling_party = communism
							ideology = ROOT
							size = 0.1
						}
					}
					90 = {
						country_event = {
							id = ww2tree.3
							days = 1
						}
					}
				}
			}
		}
	}
	prerequisite = { focus = POL1933_the_will_of_the_people }
	prerequisite = { focus = POL1933_create_militias }
}


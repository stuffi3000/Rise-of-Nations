focus = {
	id = POL_national_defence_fund
	icon = GFX_goal_generic_defence

	x = 18
	y = 0
	cost = 10
	allow_branch = {
		date > 1932.12.31
		date < 1949.1.1

	}
	available = {
		RON_can_do_focuses_1936 = yes
	}
	ai_will_do = {
		factor = 10
	}

	available_if_capitulated = yes
	search_filters = { FOCUS_FILTER_INDUSTRY }
	complete_tooltip = {
		random_state = {
			limit = { has_state_flag = POL_national_defence_fund_1 }
			add_building_construction = {
				type = arms_factory
				level = 2
				instant_build = yes
			}
		}
	}

	completion_reward = {
		random_owned_controlled_state = {
			prioritize = { 86 }
			limit = {
				free_building_slots = {
					building = arms_factory
					size > 0
					include_locked = yes
				}
			}
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = arms_factory
				level = 1
				instant_build = yes
			}
			set_state_flag = POL_national_defence_fund_1
		}

		random_owned_controlled_state = {
			prioritize = { 89 }
			limit = {
				free_building_slots = {
					building = arms_factory
					size > 0
					include_locked = yes
				}
			}
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = arms_factory
				level = 1
				instant_build = yes
			}
			set_state_flag = POL_national_defence_fund_1
		}
	}
}


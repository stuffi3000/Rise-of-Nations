focus = {
	id = POL_ukrainian_industry
	icon = GFX_focus_generic_industry_2
	prerequisite = { focus = POL_reorganize_ukraine }

	available = {
		OR = {
			any_owned_state = {
				is_core_of = UKR
				NOT = { is_core_of = POL }
			}
			UKR = {
				exists = yes
				is_subject_of = POL
			}
		}
	}

	#bypass = {
	#
	#}

	x = 1
	y = 1
	relative_position_id = POL_reorganize_ukraine
	cost = 10

	ai_will_do = {
		factor = 10
	}

	search_filters = { FOCUS_FILTER_INDUSTRY }
	complete_tooltip = {
		random_state = {
			limit = { has_state_flag = POL_ukrainian_industry_1 }
			add_building_construction = {
				type = industrial_complex
				level = 1
				instant_build = yes
			}
		}
		random_state = {
			limit = { has_state_flag = POL_ukrainian_industry_2 }
			add_building_construction = {
				type = industrial_complex
				level = 1
				instant_build = yes
			}
		}
		random_state = {
			limit = { has_state_flag = POL_ukrainian_industry_3 }
			add_building_construction = {
				type = industrial_complex
				level = 1
				instant_build = yes
			}
		}
	}

	completion_reward = {
		if = {
			limit = {
				UKR = {
					exists = yes
					is_subject_of = POL
				}
			}
			set_temp_variable = { will_get = UKR }
		}
		else = {
			set_temp_variable = { will_get = POL }
		}
		var:will_get = {
			random_owned_controlled_state = {
				prioritize = { 202 }
				limit = {
					free_building_slots = {
						building = industrial_complex
						size > 0
						include_locked = yes
					}
					NOT = { has_state_flag = POL_ukrainian_industry_2 }
					NOT = { has_state_flag = POL_ukrainian_industry_3 }
					is_core_of = UKR
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = industrial_complex
					level = 1
					instant_build = yes
				}
				set_state_flag = POL_ukrainian_industry_1
			}
			random_owned_controlled_state = {
				prioritize = { 201 }
				limit = {
					free_building_slots = {
						building = industrial_complex
						size > 0
						include_locked = yes
					}
					NOT = { has_state_flag = POL_ukrainian_industry_3 }
					NOT = { has_state_flag = POL_ukrainian_industry_1 }
					is_core_of = UKR
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = industrial_complex
					level = 1
					instant_build = yes
				}
				set_state_flag = POL_ukrainian_industry_2
			}
			random_owned_controlled_state = {
				prioritize = { 193 }
				limit = {
					free_building_slots = {
						building = industrial_complex
						size > 0
						include_locked = yes
					}
					NOT = { has_state_flag = POL_ukrainian_industry_2 }
					NOT = { has_state_flag = POL_ukrainian_industry_1 }
					is_core_of = UKR
				}
				add_extra_state_shared_building_slots = 1
				add_building_construction = {
					type = industrial_complex
					level = 1
					instant_build = yes
				}
				set_state_flag = POL_ukrainian_industry_3
			}
		}
	}
}


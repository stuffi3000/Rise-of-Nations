		add_political_power = -10
		add_timed_idea = {
			idea = BondsToPoor
			days = 50
		}
		if = {
			limit = {
				check_variable = { POL_INT_MIDDLE_SUPPORT > 97 }
			}
			set_variable = { POL_INT_PEASENT_SUPPORT = 0 }
			set_variable = { POL_INT_MIDDLE_SUPPORT = 100 }

			else = {

				add_to_variable = { POL_INT_PEASENT_SUPPORT = -3 }
				add_to_variable = { POL_INT_MIDDLE_SUPPORT = 3 }
			}
		}
		add_popularity = {
			ideology = communism
			popularity = 0.05
		}
	}
}


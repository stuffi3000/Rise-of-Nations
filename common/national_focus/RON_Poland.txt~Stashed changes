		NOT = { country_exists = JAP }
		JAP = { is_puppet = yes }
		has_war_with = JAP
	}

	x = 2
	y = 1
	relative_position_id = POL_liberalism
	cost = 10

	ai_will_do = {
		factor = 10
	}

	completion_reward = {
		add_opinion_modifier = { target = JAP modifier = large_increase }
		JAP = {
			add_opinion_modifier = { target = ROOT modifier = large_increase }
		}
		if = {
			limit = {
				JAP = {
					is_in_faction = no
				}
			}
			JAP = { country_event = { id = polmod.54 days = 1 } }
		}

		hidden_effect = {
			if = {
				limit = {
					JAP = {
						is_in_faction = no
					}
				}
				JAP = {
					add_ai_strategy = {
						type = alliance
						id = "POL"
						value = 200
					}
				}
			}
		}
	}

focus = {
	id = POL_develop_kresy
	icon = GFX_goal_generic_construct_infrastructure
	prerequisite = { focus = POL_fill_the_railways_gaps }
	x = 0
	y = 2
	relative_position_id = POL_fill_the_railways_gaps
	cost = 10

	ai_will_do = {
		factor = 10
	}

	bypass = {
		custom_trigger_tooltip = {
			tooltip = infrastructure_effort_tt
			all_owned_state = {
				free_building_slots = {
					building = infrastructure
					size < 1
				}
			}
		}
	}

	available_if_capitulated = yes
	search_filters = { FOCUS_FILTER_INDUSTRY }

	completion_reward = {
		every_state = {
			limit = {
				OR = {
					state = 784
					state = 96
					state = 95
					state = 94
					state = 93
					state = 91
					state = 89
				}
			}
			if = {
				limit = {
					infrastructure < 6
				}
				set_building_level = {
					type = infrastructure
					level = 6
					instant_build = yes
				}
				add_extra_state_shared_building_slots = 1
			}
			custom_effect_tooltip = set_infrastructure_to_6
		}
		784 = {
			add_extra_state_shared_building_slots = 3
			add_building_construction = {
				type = infrastructure
				level = 1
				instant_build = yes
			}
		}
		91 = {
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = infrastructure
				level = 1
				instant_build = yes
			}
		}
		95 = {
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = infrastructure
				level = 1
				instant_build = yes
			}
		}
	}
}


		prerequisite = { focus = NOR_nordic_joint_defense_focus }
		x = 0
		y = 6
		relative_position_id = NOR_three_brothers
		cost = 5

		ai_will_do = {
			factor = 10
		}

		available = {
			is_in_faction_with = DEN
			is_in_faction_with = SWE
			is_in_faction_with = FIN
			is_subject = no
			has_offensive_war = no
			NOT = {
				has_war_with = EST
				has_war_with = IRE
			}
		}

		search_filters = { FOCUS_FILTER_POLITICAL }

		completion_reward = {
			every_country = {
				limit = {
					is_in_faction_with = ROOT
					NOT = { original_tag = NOR }
				}
				set_country_flag = NOR_was_in_nordic_faction
			}
			dismantle_faction = yes
			create_faction = "NOR_brotherhood_of_nations"
			every_country = {
				limit = {
					has_country_flag = NOR_was_in_nordic_faction
				}
				country_event = naw_norway.25
			}
			every_country = {
				limit = {
					has_country_flag = NOR_was_in_nordic_faction
				}
				clr_country_flag = NOR_was_in_nordic_faction
			}
			if = {
				limit = {
					EST = {
						is_subject = no
						NOT = { has_war_with = NOR }
					}
				}
				EST = { country_event = naw_norway.22 }
				###########
				#hidden_effect = {
				#	news_event = { hours = 6 id = news.177 }
				#}
				#########
			}
			if = {
				limit = {
					IRE = {
						is_subject = no
						NOT = { has_war_with = NOR }
					}
				}
				IRE = { country_event = naw_norway.22 }
				###########
				#hidden_effect = {
				#	news_event = { hours = 6 id = news.177 }
				#}
				#########
			}
			custom_effect_tooltip = NOR_the_brotherhood_of_nations_tt
		}
	}


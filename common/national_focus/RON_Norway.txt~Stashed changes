					modify_tech_sharing_bonus = {
						id = comintern_research
						bonus = 0.05
					}
				}
			}
			add_tech_bonus = {
				name = armor_bonus
				bonus = 1.0
				uses = 1
				category = armor
			}
			add_tech_bonus = {
				name = codebreaking_bonus
				bonus = 1.0
				uses = 1
				category = decryption_tech
				category = encryption_tech
			}
			add_tech_bonus = {
				name = industrial_bonus
				bonus = 1.0
				uses = 1
				category = industry
			}
			hidden_effect = {
				every_country = {
					limit = {
						is_subject_of = NOR
						NOT = { has_war_with = SOV }
						num_tech_sharing_groups < 3
					}
					add_to_tech_sharing_group = comintern_research
				}
			}
		}
	}


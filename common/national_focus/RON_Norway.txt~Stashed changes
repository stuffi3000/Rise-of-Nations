		id = NOR_nordic_joint_defense_focus
		icon = GFX_goal_generic_position_armies
		prerequisite = { focus = NOR_expand_nor_research focus = NOR_invite_fin focus = NOR_the_fennoscandian_deal }
		x = 0
		y = 5
		relative_position_id = NOR_three_brothers
		cost = 5

		ai_will_do = {
			factor = 10
			modifier = {
				#EAI
				factor = 0
				is_historical_focus_on = yes
			}
		}

		available = {
			is_subject = no
			OR = {
				AND = {
					is_in_faction_with = DEN
					is_in_faction_with = SWE
				}
				AND = {
					is_in_faction_with = DEN
					is_in_faction_with = FIN
				}
				AND = {
					is_in_faction_with = SWE
					is_in_faction_with = FIN
				}
			}
		}

		search_filters = { FOCUS_FILTER_POLITICAL }

		completion_reward = {
			custom_effect_tooltip = NOR_nordic_joint_defense_focus_tt
			if = {
				limit = {
					has_idea = NOR_continuous
				}
				swap_ideas = {
					remove_idea = NOR_continuous
					add_idea = NOR_nordic_balanced_policy
				}
			}
			if = {
				limit = {
					has_idea = NOR_pro-rearmament_politics
				}
				swap_ideas = {
					remove_idea = NOR_pro-rearmament_politics
					add_idea = NOR_nordic_defense_policy
				}
			}
			if = {
				limit = { DEN = { is_in_faction_with = NOR } }
				DEN = {
					add_ideas = NOR_nordic_joint_defense
				}
			}
			if = {
				limit = { SWE = { is_in_faction_with = NOR } }
				SWE = {
					add_ideas = NOR_nordic_joint_defense
				}
			}
			if = {
				limit = { FIN = { is_in_faction_with = NOR } }
				FIN = {
					add_ideas = NOR_nordic_joint_defense
				}
			}
		}
	}


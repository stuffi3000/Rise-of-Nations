			NOT = { has_war_with = SOV }
			is_in_faction_with = SOV
		}
		search_filters = { FOCUS_FILTER_RESEARCH }


		completion_reward = {
			custom_effect_tooltip = NOR_sov_research_tt
			if = {
				limit = { SOV = { is_in_faction_with = NOR } }
				SOV = { country_event = naw_norway.19 }
			}
		}
	}

	focus = {
		id = NOR_expand_sov_research
		icon = GFX_focus_generic_socialist_science
		prerequisite = { focus = NOR_sov_research }
		x = 0
		y = 3
		relative_position_id = NOR_go_communist
		cost = 10


	## GENERIC WW1 BRANCHES


	focus = {
		id = RUE1_free_market
		icon = GFX_focus_generic_the_giant_wakes
		x = 44
		y = 0
		cost = 10

		allow_branch = {
			date > 1909.12.31 date < 1919.1.1
		}

		mutually_exclusive = { focus = RUE1_planned_economy }
		completion_reward = {
			add_ideas = { free_trade }
			add_ideas = { RI_free_economy }
		}
	}

	focus = {
		id = RUE1_support_middle_class
		icon = GFX_focus_generic_self_management
		x = -1
		y = 1
		relative_position_id = RUE1_free_market
		cost = 10
		prerequisite = { focus = RUE1_free_market }
		completion_reward = {
			random_owned_state = {
				limit = {
					free_building_slots = {
						building = industrial_complex
						size > 0
						include_locked = yes
					}
					OR = {
						is_in_home_area = yes
						NOT = {
							owner = {

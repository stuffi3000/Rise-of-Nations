focus_tree = {
	id = RON_Colombia

	country = {
		factor = 0

		modifier = {
			add = 10
			tag = COL
		}
		modifier = {
			add = 10
			original_tag = COL
		}
	}

	default = no

	reset_on_civilwar = no

	continuous_focus_position = { x = 50 y = 1100 }

	################################################################################################################################

			AND = {
				NOT = { has_government = fascism }
				NOT = { has_government = communism }
			}
		}
		completion_reward = {
			add_political_power = 150
		}
		mutually_exclusive = { focus = COL1933_the_will_of_the_people  focus = COL1933_fascism_leads_the_way }
		ai_will_do = { factor = 100 }
	}


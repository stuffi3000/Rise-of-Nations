focus = {
	id = ENG_DNC_dockyards
	icon = GFX_goal_generic_construct_naval_dockyard
	x = -4
	y = 2
	relative_position_id = ENG_DNC_new_navy
	cost = 10
	prerequisite = {
		focus = ENG_DNC_battlecruisers
		focus = ENG_DNC_patrol_boats
		focus = ENG_DNC_new_cruisers
		focus = ENG_DNC_grandest_ships
	}
	complete_tooltip = {
		navy_experience = 25
		add_extra_state_shared_building_slots = 2
		add_building_construction = {
			type = dockyard
			level = 2
			instant_build = yes
		}
	}
	completion_reward = {
		navy_experience = 25
		if = {
			limit = {
				NOT = {
					any_owned_state = {
						dockyard > 0
						free_building_slots = {
							building = dockyard
							size > 2
							include_locked = yes
						}
					}
				}
				any_owned_state = {
					is_coastal = yes
				}
			}
			random_owned_state = {
				limit = {
					is_coastal = yes
					free_building_slots = {
						building = dockyard
						size > 2
						include_locked = yes
					}
				}
				add_extra_state_shared_building_slots = 2
				add_building_construction = {
					type = dockyard
					level = 2
					instant_build = yes
				}
			}
			set_country_flag = naval_effort_built
		}
		if = {
			limit = {
				NOT = {
					has_country_flag = naval_effort_built
				}
				any_owned_state = {
					dockyard > 0
					free_building_slots = {
						building = dockyard
						size > 2
						include_locked = yes
					}
				}
			}
			random_owned_state = {
				limit = {
					dockyard > 0
					free_building_slots = {
						building = dockyard
						size > 2
						include_locked = yes
					}
				}
				add_extra_state_shared_building_slots = 2
				add_building_construction = {
					type = dockyard
					level = 2
					instant_build = yes
				}
			}
			set_country_flag = naval_effort_built
		}
	}
}

focus = {
	id = ENG_DNC_standardise_designs
	icon = GFX_focus_generic_combined_arms
	x = -1
	y = 7
	relative_position_id = ENG_DNC_army_efforts
	cost = 10
	prerequisite = {
		focus = ENG_DNC_endless_advance
		focus = ENG_DNC_endless_defense
	}
	completion_reward = {
		random_owned_state = {
			limit = {
				free_building_slots = {
					building = arms_factory
					size > 0
					include_locked = yes
				}
				OR = {
					is_in_home_area = yes
					NOT = {
						owner = {
							any_owned_state = {
								free_building_slots = {
									building = arms_factory
									size > 0
									include_locked = yes
								}
								is_in_home_area = yes
							}
						}
					}
				}
			}
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = arms_factory
				level = 1
				instant_build = yes
			}
		}
		add_ideas = {
			RI_standards
		}
	}
}

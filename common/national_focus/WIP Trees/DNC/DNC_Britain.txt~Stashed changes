#Focus for The Angevin Empire
focus = {
	id = ENGDNC_the_angevin_empire
	icon = GFX_focus_hun_elect_a_king
	x = 60
	y = 10
	cost = 10
	available_if_capitulated = yes
	prerequisite = {
		focus = ENGDNC_a_unified_citizenship
	}
	available = {
		OR = {
			NOT = {
				country_exists = FRA
			}
			FRA = {
				is_subject_of = ENG
			}
			has_completed_focus = ENGDNC_a_windsorbourbon_marriage
		}
	}
	prerequisite = {
		focus = ENGDNC_our_two_official_languages
	}
	ai_will_do = {
		factor = 1
	}
	completion_reward = {
		hidden_effect = {
			if = {
				limit = {
					country_exists = VIF
					VIF = {
						has_cosmetic_tag = ENG_PLATAGENET
					}
				}
				ENG = {
					annex_country = { target = VIF transfer_troops = yes }
				}
			}
		}
		activate_decision = RON_Formable_Decision28
	}
}

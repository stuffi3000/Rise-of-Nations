#Focus for Acquire Weapons
focus = {
	id = ENGDNC_acquire_weapons
	icon = GFX_goal_generic_army_artillery2
	x = 36
	y = 24
	cost = 10
	available_if_capitulated = yes
	prerequisite = { focus = ENGDNC_prepare_for_the_worst }
	ai_will_do = {
		factor = 1
	}
	completion_reward = {
		random_owned_state = {
			limit = {
				free_building_slots = {
					building = arms_factory
					size > 0
					include_locked = yes
				}
				OR = {
					is_in_home_area = yes
					NOT = {
						owner = {
							any_owned_state = {
								free_building_slots = {
									building = arms_factory
									size > 0
									include_locked = yes
								}
								is_in_home_area = yes
							}
						}
					}
				}
			}
			add_extra_state_shared_building_slots = 1
			add_building_construction = {
				type = arms_factory
				level = 1
				instant_build = yes
			}
		}
		add_ideas = {
			RI_more_weapons
		}
	}
}

#Focus for Sign The Treaty of Vereeniging
focus = {
	id = ENGDNC_sign_the_treaty_of_vereeniging
	icon = GFX_focus_hol_the_foundations_of_defense
	x = 21
	y = 10
	cost = 10
	available = {
		OR = {
			NOT = {
				country_exists = OFR
				country_exists = SRE
			}
			AND = {
				OFR = {
					is_subject_of = ENG

				}
				SRE = {
					is_subject_of = ENG
				}
			}
		}
	}
	available_if_capitulated = yes
	prerequisite = { focus = ENGDNC_end_the_boer_war }
	ai_will_do = {
		factor = 1
	}
	completion_reward = {
		if = {
			limit = {
				country_exists = SRE
			}
			annex_country = { target = SRE }
		}
		if = {
			limit = {
				country_exists = OFR
			}
			annex_country = {
				target = OFR
			}
		}
		every_state = {
			limit = {
				OR = {
					is_core_of = SRE
					is_core_of = OFR
				}
			}
			add_compliance = 5
		}
	}
}

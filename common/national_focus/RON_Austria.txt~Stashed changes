				create_wargoal = { type = puppet_wargoal_focus target = YUG }
			}
			if = {
				limit = {
					HUN = { NOT = { has_government = communism } }
				}
				create_wargoal = { type = puppet_wargoal_focus target = HUN }
			}
			if = {
				limit = {
					ROM = { NOT = { has_government = communism } }
				}
				create_wargoal = { type = puppet_wargoal_focus target = ROM }
			}
			if = {
				limit = {
					BUL = { NOT = { has_government = communism } }
				}
				create_wargoal = { type = puppet_wargoal_focus target = BUL }
			}
			if = {
				limit = {
					ALB = { NOT = { has_government = communism } }
				}
				create_wargoal = { type = puppet_wargoal_focus target = ALB }
			}
			if = {
				limit = {
					GRE = { NOT = { has_government = communism } }
				}
				create_wargoal = { type = puppet_wargoal_focus target = GRE }
			}
		}
	}

	focus = {
		id = AUS_war_against_the_stalinists
		icon = GFX_goal_generic_army_doctrines
		prerequisite = { focus = AUS_the_balkan_revolution }
		x = 63
		y = 7
		cost = 10
		ai_will_do = {
			factor = 1
		}

		available = {
			has_government = communism
			country_exists = SOV
			SOV = {
				has_government = communism
			}
		}



		completion_reward = {
			SOV = {
				remove_from_faction = AUS
			}
			AUS = {
				create_faction = "austria_communist_faction"
				declare_war_on = {
					target = SOV
					type = puppet_wargoal_focus
				}
			}
		}
	}

	focus = {
		id = AUS_fund_the_french_revolution
		icon = GFX_goal_generic_army_artillery2
		prerequisite = { focus = AUS_break_the_fascist_shackles }
		x = 65
		y = 6
		cost = 10
		ai_will_do = {
			factor = 1
		}

		available = {
			has_government = communism
			FRA = {
				NOT = { has_government = communism }
			}
		}



		completion_reward = {
			FRA = {
				add_ideas = communist_influence
			}
		}
	}

	focus = {
		id = AUS_start_the_french_revolution
		icon = GFX_goal_generic_major_war
		prerequisite = { focus = AUS_fund_the_french_revolution }
		x = 65
		y = 7
		cost = 10
		ai_will_do = {
			factor = 1
		}

		available = {
			has_government = communism
			FRA = {
				NOT = { has_government = communism }
				communism > 0.4
			}
		}



		completion_reward = {
			FRA = {
				start_civil_war = { ideology = communism size = 0.5 }
			}
			if = {
				limit = {
					AUS = {
						is_faction_leader = yes
					}
				}
				random_other_country = {
					limit = {
						original_tag = FRA
						has_government = communism
					}
					AUS = {
						add_to_faction = PREV
					}
				}
			}
		}
	}

	focus = {
		id = AUS_the_polish_revolution
		icon = GFX_goal_generic_attack_allies
		prerequisite = { focus = AUS_start_the_french_revolution focus = AUS_war_against_the_stalinists }
		x = 64
		y = 8
		cost = 10
		ai_will_do = {
			factor = 1
		}

		available = {
			has_government = communism
			NOT = { has_war_with = POL }
			POL = {
				NOT = {
					has_government = communism
					is_in_faction_with = AUS
				}
			}
		}



		completion_reward = {
			create_wargoal = {
				type = puppet_wargoal_focus
				target = POL
			}
		}
	}
}

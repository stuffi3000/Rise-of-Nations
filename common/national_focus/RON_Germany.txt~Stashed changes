focus_tree = {
	id = german_focus
	country = {
		factor = 0
		modifier = {
			add = 10
			tag = GER
		}
	}
	default = no
	continuous_focus_position = {
		x = 200
		y = 3000
	}

	###############################################################################################################################
	###############################################################################################################################
	###############################################################################################################################
	###############################################################################################################################
	##########DAWN OF A NEW CENTURY FOCUS##########################################################################################
	#################1900-1909#####################################################################################################
	###############################################################################################################################
	###############################################################################################################################
	###############################################################################################################################
	###############################################################################################################################

	#Focus for African Expansion
	focus = {
		id = GERDNC_african_expansion
		icon = GFX_goal_tfv_saf_support_german_coup
		x = 52
		y = 6
		cost = 10
		available_if_capitulated = yes
		prerequisite = {
			focus = GERDNC_german_empire
			focus = GERDNC_prussia
			focus = GERDNC_deutscher_bund
			focus = GERDNC_erfurt_union
			focus = GERDNC_holy_roman_empire
		}
		ai_will_do = {
			factor = 1
		}
		completion_reward = {
			custom_effect_tooltip = MOR_increase_GER_influence_tt
			MOR = {
				add_to_variable = { MOR_GER_influence = 10 }
			}
		}
	}

	#Focus for German Industrial Giant
	focus = {
		id = GERDNC_german_industrial_giant
		icon = GFX_goal_generic_construct_civ_factory
		x = 4
		y = 10
		cost = 10
		available_if_capitulated = yes
		prerequisite = { focus = GERDNC_expand_railways }
		ai_will_do = {
			factor = 1
		}
		completion_reward = {
			random_owned_state = {
				limit = {
					is_core_of = GER
				}
				add_building_construction = {
					type = industrial_complex
					level = 2
					instant_build = yes
				}
			}
		}
	}

	#Focus for Open The Political Sphere

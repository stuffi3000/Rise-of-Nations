	focus = {
		id = GER_soviet_invasion
		icon = GFX_focus_attack_soviet
		prerequisite = {
			focus = GER_poland_friend
			focus = GER_war_with_poland
			focus = GER_danzig_or_war
		}
		mutually_exclusive = {
			focus = GER_soviet_ally
		}
		bypass = {
			OR = {
				SOV = {
					is_in_faction_with = GER
				}
				NOT = {
					country_exists = SOV
				}
				GER = {
					has_war_with = SOV
				}
			}
		}
		available = {
			country_exists = SOV
			OR = {
				is_ai = no
				date > 1941.1.1
			}
		}
		x = 35
		y = 6
		cost = 10
		historical_ai = {
			#EAI
			has_government = fascism
			date > 1941.4.1
			NOT = {
				#has not lost
				GER = {
					has_capitulated = yes
				}
				GER = {
					is_subject = yes
				}
			}
			#army requirement
			has_army_manpower = {
				size > 2500000
			}
			OR = {
				has_army_size = {
					type = armor
					size > 20
				}
				has_army_manpower = {
					type = armor
					size > 200000
				}
				date > 1941.8.1
			}
			OR = {
				#borders sov
				any_neighbor_country = {
					tag = SOV
				}
				any_allied_country = {
					is_neighbor_of = ROOT
					any_neighbor_country = {
						tag = SOV
					}
				}
			}
			#sov is a threat
			SOV = {
				EAI_is_threat = yes
			}
			#no current enemies to deal with
			NOT = {
				any_home_area_neighbor_country = {
					has_war_with = ROOT
				}
			}
			OR = {
				#pol is no threat
				POL = {
					NOT = {
						EAI_is_threat = yes
					}
				}
				NOT = {
					any_home_area_neighbor_country = {
						has_war_with = ROOT
					}
				}
			}
			OR = {
				#fra is no threat
				FRA = {
					NOT = {
						EAI_is_threat = yes
					}
				}
				NOT = {
					any_home_area_neighbor_country = {
						has_war_with = ROOT
					}
				}
			}
			OR = {
				#den is no threat
				DEN = {
					NOT = {
						EAI_is_threat = yes
					}
				}
				NOT = {
					any_home_area_neighbor_country = {
						has_war_with = ROOT
					}
				}
			}
			OR = {
				#yug is no threat
				YUG = {
					NOT = {
						EAI_is_threat = yes
					}
				}
				NOT = {
					any_home_area_neighbor_country = {
						has_war_with = ROOT
					}
				}
			}
		}
		ai_will_do = {
			factor = 1000
			modifier = {
				factor = 0                #AI should not pick this focus if it does not share a border with the Soviets
				NOT = {
					any_home_area_neighbor_country = {
						tag = SOV
					}
				}
			}
			modifier = {
				factor = 0
				is_historical_focus_on = yes
				date < 1941.1.1
			}
		}
		completion_reward = {
			create_wargoal = {
				type = take_state_focus
				target = SOV
				generator = {
					219
				}
			}
		}
	}

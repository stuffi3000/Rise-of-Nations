	focus = {
		id = GER_weserubung
		icon = GFX_goal_generic_more_territorial_claims
		prerequisite = {
			focus = GER_anschluss
			focus = GER_czech_friend
			focus = GER_demand_sudetenland
		}
		will_lead_to_war_with = DEN
		will_lead_to_war_with = NOR
		bypass = {
			AND = {
				OR = {
					DEN = {
						is_in_faction_with = GER
						NOT = {
							country_exists = DEN
						}
					}
				}
			}
		}
		available = {
			POL = {
				NOT = {
					controls_state = 85
				}
				NOT = {
					controls_state = 10
				}
			}
			OR = {
				has_war_with = ENG
				NOT = {
					country_exists = ENG
				}
			}
		}
		x = 18
		y = 3
		cost = 10
		historical_ai = {
			#EAI
			has_government = fascism
			date > 1939.8.1
			NOT = {
				#has not lost
				GER = {
					has_capitulated = yes
				}
				GER = {
					is_subject = yes
				}
			}
			#army requirement
			has_army_manpower = {
				size > 1000000
			}
			OR = {
				#borders den
				any_neighbor_country = {
					tag = DEN
				}
				any_allied_country = {
					is_neighbor_of = ROOT
					any_neighbor_country = {
						tag = DEN
					}
				}
			}
			#den is a threat
			DEN = {
				EAI_is_threat = yes
			}
			#pol is no threat
			OR = {
				POL = {
					NOT = {
						EAI_is_threat = yes
					}
				}
				AND = {
					POL = {
						num_of_controlled_states < 3
					}
					POL = {
						has_army_manpower = {
							size < 200000
						}
					}
				}
				NOT = {
					any_home_area_neighbor_country = {
						has_war_with = ROOT
					}
				}
			}
			OR = {
				#is not at war with or invading sov
				NOT = {
					GER = {
						focus_progress = {
							focus = GER_soviet_invasion
							progress > 0
						}
					}
					has_war_with = SOV
				}
				#or sov is not a threat
				SOV = {
					NOT = {
						EAI_is_threat = yes
					}
				}
			}
			OR = {
				#is not at war with or invading west

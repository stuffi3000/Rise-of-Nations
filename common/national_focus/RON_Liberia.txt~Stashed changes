	}

	focus = {
		id = LIB_arms_effort
		icon = GFX_goal_generic_small_arms
		prerequisite = { focus = LIB_organize_frontier_force }
		x = 2
		y = 1
		relative_position_id = LIB_organize_frontier_force
		cost = 10

		available_if_capitulated = yes

		completion_reward = {
			add_tech_bonus = {
				name = infantry_artillery_bonus
				bonus = 1.0
				uses = 1
				category = infantry_weapons
				category = artillery
			}
		}
	}
}

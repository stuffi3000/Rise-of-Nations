	sound_effect = complete_focus
	set_country_flag = unform_available
	every_state = { limit = { is_core_of = WGR } add_core_of = ROOT }
	every_state = { limit = { is_core_of = DDR } add_core_of = ROOT }
	every_state = { limit = { is_core_of = PRS } add_core_of = ROOT }

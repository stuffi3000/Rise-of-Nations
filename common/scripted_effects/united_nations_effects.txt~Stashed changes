create_united_nations_effect = {
	set_global_flag = united_nations_created

	if = {
		limit = {
			has_game_rule = {
				rule = enable_united_nations_at_start
				option = ENABLE
			}
		}
		if = {
			limit = {
				date > 1945.10.23
			}
			# Joining of countries:
			if = {
				limit = {
					date > 1945.10.23
				}
				# Founding Members:
				if = {
					limit = {
						country_exists = ARG
					}
					ARG = {
						join_united_nations_effect = yes
					}
				}
				if = {
					limit = {
						country_exists = BLR
					}
					BLR = {
						join_united_nations_effect = yes
					}
				}
				if = {
					limit = {
						country_exists = BRA
					}
					BRA = {
						join_united_nations_effect = yes
					}
				}
				if = {
					limit = {
						country_exists = BRA
					}
					BRA = {
						join_united_nations_effect = yes
					}
				}
				if = {
					limit = {
						country_exists = CHL
					}
					CHL = {
						join_united_nations_effect = yes
					}
				}
				if = {
					limit = {

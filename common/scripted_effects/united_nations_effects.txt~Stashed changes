				}
			}
			every_country = {
				limit = {
					has_country_flag = member_of_united_nations
					any_other_country = {
						has_country_flag = member_of_united_nations
						NOT = { has_country_flag = united_nations_opinion_set_for_@PREV }
					}
				}
				every_country = {
					limit = {
						NOT = { tag = PREV }
						has_country_flag = member_of_united_nations
						NOT = { has_country_flag = united_nations_opinion_set_for_@PREV }
					}
					add_opinion_modifier = {
						target = PREV
						modifier = UNSC_member_opinion
					}
					set_country_flag = united_nations_opinion_set_for_@PREV
				}
			}

			# Set Focus at start:
			#if = {
			#	limit = {
			#		date > 1939.08.13
			#	}
			#	set_global_flag = league_of_nations_focus_1_selected
			#	set_global_flag = league_of_nations_focus_1_completed
			#	set_variable = { global.frame_of_focus_links_1 = 1 }
			#	random_country = {
			#		limit = {
			#			has_country_flag = leader_of_league_of_nations
			#		}
			#		add_political_power = 75
			#	}
			#	every_country = {
			#		limit = {
			#			has_country_flag = member_of_league_of_nations
			#		}
			#		add_ideas = league_of_nations_focus_idea_1
			#	}
			#}
		}
	}
}

join_united_nations_effect = {
	if = {
		limit = {
			NOT = { has_country_flag = member_of_united_nations }
			NOT = {
				tag = LON
				tag = UNA
			}
		}
		set_country_flag = member_of_united_nations
		add_to_array = { global.members_of_united_nation_arrays = THIS.id }
		add_ideas = RON_United_Nations
		diplomatic_relation = {
			country = UNA
			relation = guarantee
			active = yes

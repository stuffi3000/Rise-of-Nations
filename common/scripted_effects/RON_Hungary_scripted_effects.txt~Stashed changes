create_politician_istvan_horthy = {
	if = {
		limit = {
			OR = {
				has_country_flag = HUN_had_war
				has_war = yes
			}
		}
		create_country_leader = {
			name = "István Horthy"
			desc = "POLITICS_ISTVAN_HORTHY_DESC"
			picture = "Portrait_Hungary_Istvan_Horthy.dds"
			expire = "1965.1.1"
			ideology = moderatism
			traits = { popular_figurehead2 former_ace_pilot }
		}
		else = {
			create_country_leader = {
				name = "István Horthy"
				desc = "POLITICS_ISTVAN_HORTHY_DESC"
				picture = "Portrait_Hungary_Istvan_Horthy.dds"
				expire = "1965.1.1"
				ideology = moderatism
				traits = { popular_figurehead2 }
			}
		}
	}
}


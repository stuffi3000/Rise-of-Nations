RON_country_setup = {
	if = {
		limit = {
			RON_party_names_change = yes
		}
		RON_handle_party_names = yes
	}

	if = {
		limit = {
			RON_bengali_speaker = yes
		}
		add_ideas = RON_Bengali
	}
	if = {
		limit = {
			RON_chinese_speaker = yes
		}
		add_ideas = RON_Chinese
	}
	if = {
		limit = {
			RON_english_speaker = yes
		}
		add_ideas = RON_English
	}
	if = {
		limit = {
			RON_french_speaker = yes
		}
		add_ideas = RON_French
	}
	if = {
		limit = {
			RON_german_speaker = yes
		}
		add_ideas = RON_German
	}
	if = {
		limit = {
			RON_arabic_speaker = yes
		}
		add_ideas = RON_Arabic
	}
	if = {
		limit = {
			RON_hindu_speaker = yes
		}
		add_ideas = RON_Hindu
	}
	if = {
		limit = {
			RON_italian_speaker = yes
		}
		add_ideas = RON_Italian
	}
	if = {
		limit = {
			RON_japanese_speaker = yes
		}
		add_ideas = RON_Japanese
	}
	if = {

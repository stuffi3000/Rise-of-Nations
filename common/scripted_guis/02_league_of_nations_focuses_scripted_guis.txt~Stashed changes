scripted_gui = {
	league_of_nations_focuses_window = {
		context_type = player_context

		window_name = "league_of_nations_focuses_window"

		visible = {
			OR = {
				AND = {
					is_ai = no
					check_variable = { show_league_of_nations_focuses > 0 }
				}
				AND = {
					is_ai = yes
					has_country_flag = leader_of_league_of_nations
				}
			}
			any_country = {
				has_country_flag = leader_of_league_of_nations
			}
		}

		effects = {
			close_league_of_nations_focuses_window_button_click = {
				set_variable = { show_league_of_nations_focuses = 0 }
				set_variable = { show_selected_league_of_nations_focus = 0 }
			}
			league_of_nations_focus_title_button_click = {
				if = {
					limit = {
						has_country_flag = leader_of_league_of_nations
					}
					set_variable = { show_selected_league_of_nations_focus = v }
				}
			}
			league_of_nations_focus_icon_button_click = {
				if = {
					limit = {
						has_country_flag = leader_of_league_of_nations
					}
					set_variable = { show_selected_league_of_nations_focus = v }
				}
			}
		}

		triggers = {
			league_of_nations_focus_title_button_visible = {
				OR = {
					check_variable = { v = 1 }
					check_variable = { v = 2 }
					check_variable = { v = 3 }
					check_variable = { v = 4 }
					check_variable = { v = 5 }
					check_variable = { v = 6 }
					check_variable = { v = 7 }
					check_variable = { v = 8 }
				}
			}
			league_of_nations_focus_icon_button_visible = {
				OR = {
					check_variable = { v = 1 }
					check_variable = { v = 2 }
					check_variable = { v = 3 }
					check_variable = { v = 4 }
					check_variable = { v = 5 }
					check_variable = { v = 6 }
					check_variable = { v = 7 }
					check_variable = { v = 8 }
				}
			}
			league_of_nations_focus_title_visible = {
				OR = {
					check_variable = { v = 1 }
					check_variable = { v = 2 }
					check_variable = { v = 3 }
					check_variable = { v = 4 }
					check_variable = { v = 5 }
					check_variable = { v = 6 }
					check_variable = { v = 7 }
					check_variable = { v = 8 }
				}
			}
			league_of_nations_focus_icon_button_active_visible = {
				has_country_flag = leader_of_league_of_nations
				has_global_flag = league_of_nations_focus_active
				OR = {
					AND = {
						check_variable = { v = 1 }
						has_global_flag = league_of_nations_focus_1_selected
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
					}
					AND = {
						check_variable = { v = 2 }

						has_global_flag = league_of_nations_focus_1_completed
					}
					AND = {
						check_variable = { v = 6 }
						has_global_flag = league_of_nations_focus_6_completed
					}
				}
			}
			focus_link_right_2_visible = {
				OR = {
					AND = {
						check_variable = { v = 4 }
						has_global_flag = league_of_nations_focus_4_completed
					}
				}
			}
			focus_link_right_3_visible = {
				OR = {
					AND = {
						check_variable = { v = 1 }
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
					}
					AND = {
						check_variable = { v = 6 }
						NOT = { has_global_flag = league_of_nations_focus_6_completed }
					}
				}
			}
			focus_link_right_4_visible = {
				OR = {
					AND = {
						check_variable = { v = 4 }
						NOT = { has_global_flag = league_of_nations_focus_4_completed }
					}
				}
			}
			focus_link_up_left_1_visible = {
				OR = {
					AND = {
						check_variable = { v = 3 }
						has_global_flag = league_of_nations_focus_1_completed
					}
				}
			}
			focus_link_up_left_2_visible = {
				always = no
			}
			focus_link_up_left_3_visible = {
				OR = {
					AND = {
						check_variable = { v = 3 }
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
					}
				}
			}
			focus_link_up_left_4_visible = {
				always = no
			}
			focus_link_up_right_1_visible = {
				OR = {
					AND = {
						check_variable = { v = 2 }
						has_global_flag = league_of_nations_focus_1_completed
					}
				}
			}
			focus_link_up_right_2_visible = {
				always = no
			}
			focus_link_up_right_3_visible = {
				OR = {
					AND = {
						check_variable = { v = 2 }
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
					}
				}
			}
			focus_link_up_right_4_visible = {
				always = no
			}
			focus_link_up_down_left_1_visible = {
				OR = {
					AND = {
						check_variable = { v = 5 }
						has_global_flag = league_of_nations_focus_5_completed
					}
				}
			}
			focus_link_up_down_left_2_visible = {
				always = no
			}
			focus_link_up_down_left_3_visible = {
				OR = {
					AND = {
						check_variable = { v = 5 }
						NOT = { has_global_flag = league_of_nations_focus_5_completed }
					}
				}
			}
			focus_link_up_down_left_4_visible = {
				always = no
			}
			focus_link_up_down_right_1_visible = {
				OR = {
					AND = {
						check_variable = { v = 4 }
						has_global_flag = league_of_nations_focus_4_completed
					}
				}
			}
			focus_link_up_down_right_2_visible = {
				always = no
			}
			focus_link_up_down_right_3_visible = {
				OR = {
					AND = {
						check_variable = { v = 4 }
						NOT = { has_global_flag = league_of_nations_focus_4_completed }
					}
				}
			}
			focus_link_up_down_right_4_visible = {
				always = no
			}
		}

		dynamic_lists = {
			league_of_nations_focuses_gridbox = {
				array = global.league_of_nations_focuses_array
				entry_container = league_of_nations_focuses_entry

				ai_weights = {
					league_of_nations_focus_icon_button_click = {
						ai_will_do = {
							factor = 0
						}
					}
					league_of_nations_focus_title_button_click = {
						ai_will_do = {
							factor = 0

							modifier = {
								NOT = { has_global_flag = league_of_nations_focus_active }
								OR = {
									AND = {
										check_variable = { v = 1 }
										NOT = { has_global_flag = league_of_nations_focus_1_selected }
										NOT = { has_global_flag = league_of_nations_focus_1_completed }
									}
									AND = {
										check_variable = { v = 2 }
										has_global_flag = league_of_nations_focus_1_completed
										NOT = { has_global_flag = league_of_nations_focus_2_selected }
										NOT = { has_global_flag = league_of_nations_focus_2_completed }
										NOT = { has_global_flag = league_of_nations_focus_3_selected }
										NOT = { has_global_flag = league_of_nations_focus_3_completed }
										check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
										NOT = { has_government = fascism }
										NOT = { has_government = communism }
									}
									AND = {
										check_variable = { v = 3 }
										has_global_flag = league_of_nations_focus_1_completed
										NOT = { has_global_flag = league_of_nations_focus_2_selected }
										NOT = { has_global_flag = league_of_nations_focus_2_completed }
										NOT = { has_global_flag = league_of_nations_focus_3_selected }
										NOT = { has_global_flag = league_of_nations_focus_3_completed }
										check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
										OR = {
											has_government = fascism
											has_government = communism
										}
									}
									AND = {
										check_variable = { v = 4 }
										has_global_flag = league_of_nations_focus_2_completed
										NOT = { has_global_flag = league_of_nations_focus_4_selected }
										NOT = { has_global_flag = league_of_nations_focus_4_completed }
										date > 1939.09.17
									}
									AND = {
										check_variable = { v = 5 }
										has_global_flag = league_of_nations_focus_3_completed
										NOT = { has_global_flag = league_of_nations_focus_5_selected }
										NOT = { has_global_flag = league_of_nations_focus_5_completed }
										date > 1939.09.17
									}
									AND = {
										check_variable = { v = 6 }
										OR = {
											has_global_flag = league_of_nations_focus_4_completed
											has_global_flag = league_of_nations_focus_5_completed
										}
										NOT = { has_global_flag = league_of_nations_focus_6_selected }
										NOT = { has_global_flag = league_of_nations_focus_6_completed }
										date > 1942.09.17
										check_variable = { num_of_won_league_of_nations_elections > 2 }
									}
									AND = {
										check_variable = { v = 7 }
										has_global_flag = league_of_nations_focus_4_completed
										has_global_flag = league_of_nations_focus_6_completed
										NOT = { has_global_flag = league_of_nations_focus_7_selected }
										NOT = { has_global_flag = league_of_nations_focus_7_completed }
										date > 1945.09.17
										has_war = no
									}
									AND = {
										check_variable = { v = 8 }
										has_global_flag = league_of_nations_focus_5_completed
										has_global_flag = league_of_nations_focus_6_completed
										NOT = { has_global_flag = league_of_nations_focus_8_selected }
										NOT = { has_global_flag = league_of_nations_focus_8_completed }
										date > 1945.09.17
										has_war = no
									}
								}
								add = 1
							}
						}
					}
				}
			}
		}

		ai_enabled = {
			always = yes
		}

		ai_test_interval = 24
		ai_test_variance = 0.2

		ai_check = {
			has_country_flag = leader_of_league_of_nations
			NOT = { has_global_flag = league_of_nations_focus_active }
			NOT = { has_global_flag = league_of_nations_focus_7_completed }
			NOT = { has_global_flag = league_of_nations_focus_8_completed }
		}

		ai_weights = {
			close_league_of_nations_focuses_window_button_click = {
				ai_will_do = {
					factor = 0
				}
			}
		}
	}

	league_of_nations_focus_detail_view = {
		context_type = player_context

		window_name = "league_of_nations_focus_detail_view"

		visible = {
			OR = {
				is_ai = yes
				check_variable = { show_league_of_nations_focuses > 0 }
			}
			check_variable = { show_selected_league_of_nations_focus > 0 }
		}

		effects = {
			league_of_nations_focus_detail_view_close_button_click = {
				set_variable = { show_selected_league_of_nations_focus = 0 }
			}
			league_of_nations_focus_detail_view_start_button_click = {
				if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 1 }
					}
					set_global_flag = league_of_nations_focus_1_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 2 }
					}
					set_global_flag = league_of_nations_focus_2_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 3 }
					}
					set_global_flag = league_of_nations_focus_3_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 4 }
					}
					set_global_flag = league_of_nations_focus_4_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 5 }
					}
					set_global_flag = league_of_nations_focus_5_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 6 }
					}
					set_global_flag = league_of_nations_focus_6_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 7 }
					}
					set_global_flag = league_of_nations_focus_7_selected
				}
				else_if = {
					limit = {
						check_variable = { show_selected_league_of_nations_focus = 8 }
					}
					set_global_flag = league_of_nations_focus_8_selected
				}
				set_global_flag = league_of_nations_focus_active
				set_variable = { global.time_til_league_of_nations_focus_is_completed = 210 }
				set_variable = { global.league_of_nations_focus_progress = 0 }
				set_variable = { show_selected_league_of_nations_focus = 0 }
			}
		}

		triggers = {
			league_of_nations_focus_detail_view_start_button_click_enabled = {
				NOT = { has_global_flag = league_of_nations_focus_active }
				OR = {
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 1 }
						NOT = { has_global_flag = league_of_nations_focus_1_selected }
						NOT = { has_global_flag = league_of_nations_focus_1_completed }
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 2 }
						has_global_flag = league_of_nations_focus_1_completed
						NOT = { has_global_flag = league_of_nations_focus_2_selected }
						NOT = { has_global_flag = league_of_nations_focus_2_completed }
						NOT = { has_global_flag = league_of_nations_focus_3_selected }
						NOT = { has_global_flag = league_of_nations_focus_3_completed }
						check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 3 }
						has_global_flag = league_of_nations_focus_1_completed
						NOT = { has_global_flag = league_of_nations_focus_2_selected }
						NOT = { has_global_flag = league_of_nations_focus_2_completed }
						NOT = { has_global_flag = league_of_nations_focus_3_selected }
						NOT = { has_global_flag = league_of_nations_focus_3_completed }
						check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 4 }
						has_global_flag = league_of_nations_focus_2_completed
						NOT = { has_global_flag = league_of_nations_focus_4_selected }
						NOT = { has_global_flag = league_of_nations_focus_4_completed }
						date > 1939.09.17
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 5 }
						has_global_flag = league_of_nations_focus_3_completed
						NOT = { has_global_flag = league_of_nations_focus_5_selected }
						NOT = { has_global_flag = league_of_nations_focus_5_completed }
						date > 1939.09.17
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 6 }
						OR = {
							has_global_flag = league_of_nations_focus_4_completed
							has_global_flag = league_of_nations_focus_5_completed
						}
						NOT = { has_global_flag = league_of_nations_focus_6_selected }
						NOT = { has_global_flag = league_of_nations_focus_6_completed }
						date > 1942.09.17
						check_variable = { num_of_won_league_of_nations_elections > 2 }
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 7 }
						has_global_flag = league_of_nations_focus_4_completed
						has_global_flag = league_of_nations_focus_6_completed
						NOT = { has_global_flag = league_of_nations_focus_7_selected }
						NOT = { has_global_flag = league_of_nations_focus_7_completed }
						date > 1945.09.17
						has_war = no
					}
					AND = {
						check_variable = { show_selected_league_of_nations_focus = 8 }
						has_global_flag = league_of_nations_focus_5_completed
						has_global_flag = league_of_nations_focus_6_completed
						NOT = { has_global_flag = league_of_nations_focus_8_selected }
						NOT = { has_global_flag = league_of_nations_focus_8_completed }
						date > 1945.09.17
						has_war = no
					}
				}
			}
		}

		properties = {
			league_of_nations_focus_detail_view_research_progressbar = {
				image = [This.GetLeagueOfNationsFocusProgressIconFrame]
			}
		}

		ai_enabled = {
			always = yes
		}

		ai_test_interval = 24
		ai_test_variance = 0.2

		ai_check = {
			always = yes
		}

		ai_weights = {
			league_of_nations_focus_detail_view_start_button_click = {
				ai_will_do = {
					factor = 1
				}
			}
			league_of_nations_focus_detail_view_close_button_click = {
				ai_will_do = {
					factor = 0

					modifier = {
						OR = {
							NOT = { has_country_flag = member_of_league_of_nations }
							NOT = { has_country_flag = leader_of_league_of_nations }
							has_global_flag = league_of_nations_focus_active
							NOT = {
								OR = {
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 1 }
										NOT = { has_global_flag = league_of_nations_focus_1_selected }
										NOT = { has_global_flag = league_of_nations_focus_1_completed }
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 2 }
										has_global_flag = league_of_nations_focus_1_completed
										NOT = { has_global_flag = league_of_nations_focus_2_selected }
										NOT = { has_global_flag = league_of_nations_focus_2_completed }
										NOT = { has_global_flag = league_of_nations_focus_3_selected }
										NOT = { has_global_flag = league_of_nations_focus_3_completed }
										check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 3 }
										has_global_flag = league_of_nations_focus_1_completed
										NOT = { has_global_flag = league_of_nations_focus_2_selected }
										NOT = { has_global_flag = league_of_nations_focus_2_completed }
										NOT = { has_global_flag = league_of_nations_focus_3_selected }
										NOT = { has_global_flag = league_of_nations_focus_3_completed }
										check_variable = { global.num_of_total_successful_league_of_nations_votings > 2 }
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 4 }
										has_global_flag = league_of_nations_focus_2_completed
										NOT = { has_global_flag = league_of_nations_focus_4_selected }
										NOT = { has_global_flag = league_of_nations_focus_4_completed }
										date > 1939.09.17
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 5 }
										has_global_flag = league_of_nations_focus_3_completed
										NOT = { has_global_flag = league_of_nations_focus_5_selected }
										NOT = { has_global_flag = league_of_nations_focus_5_completed }
										date > 1939.09.17
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 6 }
										OR = {
											has_global_flag = league_of_nations_focus_4_completed
											has_global_flag = league_of_nations_focus_5_completed
										}
										NOT = { has_global_flag = league_of_nations_focus_6_selected }
										NOT = { has_global_flag = league_of_nations_focus_6_completed }
										date > 1942.09.17
										check_variable = { num_of_won_league_of_nations_elections > 2 }
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 7 }
										has_global_flag = league_of_nations_focus_4_completed
										has_global_flag = league_of_nations_focus_6_completed
										NOT = { has_global_flag = league_of_nations_focus_7_selected }
										NOT = { has_global_flag = league_of_nations_focus_7_completed }
										date > 1945.09.17
										has_war = no
									}
									AND = {
										check_variable = { show_selected_league_of_nations_focus = 8 }
										has_global_flag = league_of_nations_focus_5_completed
										has_global_flag = league_of_nations_focus_6_completed
										NOT = { has_global_flag = league_of_nations_focus_8_selected }
										NOT = { has_global_flag = league_of_nations_focus_8_completed }
										date > 1945.09.17
										has_war = no
									}
								}
							}
						}
						add = 100
					}
				}
			}
		}
	}

			### 25

			EAI_config_CUSTOM_BONUS_25_button_click = {
				if = { limit = { NOT = { has_country_flag = EAI_config_COUNTRY } }

					if = { limit = { NOT = { check_variable = { global.EAI_custom_bonus_25 = 1 } } }
						set_variable = { global.EAI_custom_bonus_25 = 1 }
						every_country = { limit = { has_country_flag = EAI_dynamic_country_bonus_active }
							EAI_clear_equipment_modifier_HARD = yes
							EAI_clear_equipment_modifier_VERY_HARD = yes
							EAI_clear_equipment_modifier_INSANE = yes
						}
					}
					else_if = { limit = {} set_variable = { global.EAI_custom_bonus_25 = 0 } }
				}
				else = {

					if = { limit = { NOT = { check_variable = { EAI_custom_bonus_25_temp = 1 } } }
						set_variable = { EAI_custom_bonus_25_temp = 1 }
						var:EAI_configuration_target = {
							EAI_clear_equipment_modifier_HARD = yes
							EAI_clear_equipment_modifier_VERY_HARD = yes
							EAI_clear_equipment_modifier_INSANE = yes
						}
					}
					else_if = { limit = {} set_variable = { EAI_custom_bonus_25_temp = 0 } }
				}
			}

			### 26

			EAI_config_CUSTOM_BONUS_26_button_shift_right_click = { set_temp_variable = { EAI_selected_custom_bonus_setting = 26 } EAI_custom_bonus_shift_right_click = yes }
			EAI_config_CUSTOM_BONUS_26_button_control_right_click = { set_temp_variable = { EAI_selected_custom_bonus_setting = 26 } EAI_custom_bonus_control_right_click = yes }

scripted_gui = { #CB

	EAI_custom_bonus = {

		context_type = player_context

		window_name = "EAI_config_CUSTOM_BONUS_window"

		visible = {
			has_country_flag = EAI_config_CUSTOM_BONUS
		}

		effects = {

			EAI_config_CUSTOM_BONUS_save_button_click = {
				if = { limit = { has_country_flag = EAI_config_COUNTRY }
					set_temp_variable = { settings = 30 }
					while_loop_effect = { limit = { check_variable = { settings > 0 } }
						meta_effect = {
							text = { set_variable = { Root.EAI_custom_bonus_[x]_saved = Root.EAI_custom_bonus_[x]_temp } }
							x = "[?settings]"
						}
						subtract_from_temp_variable = { settings = 1 }
					}
				}
				else = {
					set_temp_variable = { settings = 30 }
					while_loop_effect = { limit = { check_variable = { settings > 0 } }
						meta_effect = {
							text = { set_variable = { Root.EAI_custom_bonus_[x]_saved = global.EAI_custom_bonus_[x] } }
							x = "[?settings]"
						}
						subtract_from_temp_variable = { settings = 1 }
					}
				}
			}

			EAI_config_CUSTOM_BONUS_load_button_click = {
				if = { limit = { has_country_flag = EAI_config_COUNTRY }
					set_temp_variable = { settings = 30 }
					while_loop_effect = { limit = { check_variable = { settings > 0 } }
						meta_effect = {
							text = { set_variable = { Root.EAI_custom_bonus_[x]_temp = Root.EAI_custom_bonus_[x]_saved } }
							x = "[?settings]"
						}
						subtract_from_temp_variable = { settings = 1 }
					}
				}
				else = {
					set_temp_variable = { settings = 30 }
					while_loop_effect = { limit = { check_variable = { settings > 0 } }
						meta_effect = {
							text = { set_variable = { global.EAI_custom_bonus_[x] = Root.EAI_custom_bonus_[x]_saved } }
							x = "[?settings]"
						}
						subtract_from_temp_variable = { settings = 1 }
					}
				}
			}

			### 1


### Starting templates

light_armor_role_early = {
	roles = { light_armor }
	upgrade_prio = { factor = 0 }
	match_to_count = 0.8

	EARLY_LIGHT_ARMOR_TEMPLATE_GER = {
		upgrade_prio = {
			factor = 0.01

			# FOCUS REQ.
			modifier = { factor = 0 NOT = { check_variable = { EAI_FOCUS/LAND/attachment = global.EAI_LARM_ATTACHMENT } } }
		}
		production_prio = {
			factor = 0.01

			# FOCUS REQ.
			modifier = { factor = 0 NOT = { check_variable = { EAI_FOCUS/LAND/attachment = global.EAI_LARM_ATTACHMENT } } }
		}

		# TEMPLATE
		target_width = 12
		width_weight = 1

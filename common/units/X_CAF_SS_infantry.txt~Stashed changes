	DS_motorized = {
		sprite = motorized
		map_icon_category = infantry
		special_forces = yes

		priority = 599
		ai_priority = 200
		active = no

		type = {
			motorized
		}

		group = mobile

		categories = {
			category_front_line
			category_all_infantry
			category_army
		}

		combat_width = 2

		#Size Definitions
		max_strength = 28
		max_organisation = 70
		default_morale = 0.60
		manpower = 1200

		#Misc Abilities
		defense = 0.5
		breakthrough = 0.5
		training_time = 90
		suppression = 2
		weight = 0.75
		supply_consumption = 0.11

		# this is what moves us and sets speed
		transport = motorized_equipment

		need = {
			infantry_equipment = 110
			motorized_equipment = 55
		}

		forest = {
			attack = -0.1
			movement = -0.5
		}
		mountain = {
			attack = -0.05
		}
		jungle = {
			attack = -0.2
			movement = -0.5
		}
		marsh = {
			attack = -0.1
			movement = -0.3
		}
		urban = {
			attack = -0.1
		}
		river = {
			attack = -0.1
			movement = -0.1
		}

		need = {
			light_tank_equipment = 60
		}
		manpower = 500
		max_organisation = 10
		default_morale = 0.6 # fanatics +0.3
		training_time = 180
		max_strength = 2
		weight = 1
		supply_consumption = 0.25 # internal struggle, bad allocation +0.05

		#Tanks are awesome
		soft_attack = 0.5

		forest = {
			attack = -0.2
			movement = -0.4
		}
		mountain = {
			attack = -0.1
		}
		jungle = {
			attack = -0.3
			movement = -0.4
		}

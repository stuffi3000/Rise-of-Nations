	GB_mechanized = {
		sprite = mechanized
		map_icon_category = infantry
		special_forces = yes

		priority = 610
		ai_priority = 200
		active = no

		type = {
			mechanized
		}

		group = mobile

		categories = {
			category_front_line
			category_all_infantry
			category_army
		}

		combat_width = 2

		#Offensive Abilities
		soft_attack = 0.2
		hard_attack = 4.2

		#Size Definitions
		max_strength = 33
		max_organisation = 70
		default_morale = 0.6
		manpower = 1200

		#Misc Abilities
		defense = 0.5
		breakthrough = 0.5
		training_time = 120
		suppression = 2
		weight = 1

		supply_consumption = 0.18

		# needed since we give so much bonus to infantry even with no mech equipment
		essential = {
			infantry_equipment
			mechanized_equipment
		}

		# this is what moves us and sets speed
		transport = mechanized_equipment

		need = {
			mechanized_equipment = 55
			infantry_equipment = 110
		}

		forest = {
			attack = -0.2
		}
		mountain = {
			attack = -0.05
		}
		jungle = {
			attack = -0.3
		}
		marsh = {
			attack = -0.1
		}
		urban = {
			attack = -0.2

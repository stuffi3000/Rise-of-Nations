		20 = { "Legio %s Victrix" }
		21 = { "Legio %s Rapax" }
		22 = { "Legio %s Deiotariana" }
		23 = { "Legio %s Flavia Felix" }
		24 = { "Legio %s Minervia" }
		25 = { "Legio %s Primigenia" }
		26 = { "Legio %s Parthica" }
		27 = { "Legio %s Valeria Vict." }
		28 = { "Legio %s Fretensis" }
		29 = { "Legio %s Traiana" }

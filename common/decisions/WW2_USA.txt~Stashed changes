	USA_first_protocol_start = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		visible = {
			NOT = { has_country_flag = end_first_protocol }
			USA = { has_completed_focus = USA_Lend_Lease_to_the_USSR }
		}

		cost = 0
		days_remove = 180

		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		remove_effect = {
			hidden_effect = { set_country_flag = first_protocol_done }
		}

	}

	USA_Bomber_Lend_Lease1 = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		visible = {
			has_decision = USA_first_protocol_start
			USA = { has_completed_focus = USA_Lend_Lease_to_the_USSR }
		}

		cost = 50
		days_remove = 20

		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		remove_effect = {
			send_equipment = {
				equipment = tac_bomber_equipment
				amount = 200
				target = SOV
			}
		}

	}


	USA_yorktown_class = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			#has_idea = newport_news_shipbuilding
		}

		cost = 0
		fire_only_once = yes
		#days_remove = 1
		ai_will_do = {
			factor = 100
		}

		visible = {
			NOT = { has_tech = basic_carrier }
		}

		complete_effect = {
			add_ideas = newport_news_shipbuilding
			#}

			#remove_effect = {


			if = {
				limit = { has_dlc = "Man the Guns" }
				set_technology = { basic_ship_hull_carrier = 1 }

				create_equipment_variant = {
					name = "Ranger Class"
					type = ship_hull_carrier_1
					name_group = USA_CV_HISTORICAL
					parent_version = 0
					modules = {
						fixed_ship_deck_slot_1 = ship_deck_space
						fixed_ship_deck_slot_2 = ship_deck_space
						fixed_ship_anti_air_slot = ship_anti_air_2
						fixed_ship_radar_slot = empty
						fixed_ship_engine_slot = carrier_ship_engine_2
						fixed_ship_secondaries_slot = dp_ship_secondaries
						front_1_custom_slot = ship_anti_air_2
						#mid_2_custom_slot = ship_deck_space

					}
					obsolete = yes
				}
				add_equipment_production = {
					equipment = {
						type = ship_hull_carrier_1
						creator = "USA"
						version_name = "Ranger Class"
					}
					name = "USS Ranger"
					requested_factories = 1
					progress = 0.99
					amount = 1
				}

				create_equipment_variant = {
					name = "Yorktown Class"
					type = ship_hull_carrier_1
					name_group = USA_CV_HISTORICAL
					parent_version = 0
					modules = {
						fixed_ship_deck_slot_1 = ship_deck_space
						fixed_ship_deck_slot_2 = ship_deck_space
						fixed_ship_anti_air_slot = ship_anti_air_2
						fixed_ship_radar_slot = empty
						fixed_ship_engine_slot = carrier_ship_engine_2
						fixed_ship_secondaries_slot = dp_ship_secondaries
						front_1_custom_slot = ship_deck_space
						#mid_2_custom_slot = ship_deck_space

					}
					#obsolete = yes
				}
				add_equipment_production = {
					equipment = {
						type = ship_hull_carrier_1
						creator = "USA"
						version_name = "Yorktown Class"
					}
					name = "USS Yorktown"
					requested_factories = 1
					progress = 0.93
					amount = 1
				}
				add_equipment_production = {
					equipment = {
						type = ship_hull_carrier_1
						creator = "USA"
						version_name = "Yorktown Class"
					}
					name = "USS Enterprise"
					requested_factories = 1
					progress = 0.3
					amount = 1
				}
				add_equipment_production = {
					equipment = {
						type = ship_hull_carrier_1
						creator = "USA"
						version_name = "Yorktown Class"
					}
					name = "USS Wasp"
					requested_factories = 1
					progress = 0.01
					amount = 1
				}

				else = {
					set_technology = { basic_carrier = 1 }
					create_equipment_variant = {
						name = "Yorktown class"
						type = carrier_2
						upgrades = {
							ship_deckspace_upgrade = 2
							ship_engine_upgrade = 2
							ship_reliability_upgrade = 1
						}
					}
					add_equipment_production = {
						equipment = {
							type = ship_hull_carrier_1
							creator = "USA"
							version_name = "Yorktown Class"
						}
						name = "USS Yorktown"
						requested_factories = 1
						progress = 0.93
						amount = 1
					}
					add_equipment_production = {
						equipment = {
							type = ship_hull_carrier_1
							creator = "USA"
							version_name = "Yorktown Class"
						}
						name = "USS Enterprise"
						requested_factories = 1
						progress = 0.35
						amount = 1
					}
					add_equipment_production = {
						equipment = {
							type = ship_hull_carrier_1
							creator = "USA"
							version_name = "Yorktown Class"
						}
						name = "USS Wasp"
						requested_factories = 1
						progress = 0.01
						amount = 1
					}
				}
			}
		}
	}


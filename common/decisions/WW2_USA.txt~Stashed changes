USA_canadian_issue = {

	USA_annex_canada = {

		icon = generic_prepare_civil_war

		allowed = {
			OR = {
				original_tag = USA
			}
		}

		available = {
			has_civil_war = no
			OR = {
				NOT = { country_exists = CAN }
				CAN = { is_puppet_of = ENG }
			}
			NOT = {
				ROOT = {
					owns_state = 276
					owns_state = 468
					owns_state = 465
					owns_state = 464
					owns_state = 466
					owns_state = 682
					owns_state = 683
					owns_state = 467
					owns_state = 469
					owns_state = 472
					owns_state = 470
					owns_state = 473
					owns_state = 471
					owns_state = 740
					owns_state = 739
				}
			}
		}

		cost = 50
		fire_only_once = yes
		ai_will_do = {
			factor = 0
		}

		visible = {
			OR = {
				has_completed_focus = USA_Secure_Canada
			}
		}

		complete_effect = {
			ROOT = {
				transfer_state = 276
				transfer_state = 468
				transfer_state = 465
				transfer_state = 464
				transfer_state = 466
				transfer_state = 682
				transfer_state = 683
				transfer_state = 467
				transfer_state = 469
				transfer_state = 472
				transfer_state = 470
				transfer_state = 473
				transfer_state = 471
				transfer_state = 740
				transfer_state = 739

			}
		}
	}

	USA_join_the_united_states = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = CAN
		}

		available = {
			has_government = democratic
		}

		cost = 10
		fire_only_once = yes
		ai_will_do = {
			factor = 0
		}

		visible = {
			OR = {
				has_completed_focus = USA_Secure_Canada
			}
		}

		complete_effect = {
			if = {
				limit = { USA = { has_completed_focus = USA_Secure_Canada } }

				USA = { annex_country = { target = CAN } }
			}
		}
	}


}







USA_trade_embargo = {

	USA_trade_embargo_against_japan = {

		icon = generic_prepare_civil_war

		allowed = {
			OR = {
				original_tag = USA
				original_tag = FRA
				original_tag = ENG
				original_tag = SOV
				original_tag = HOL
				original_tag = FRA
			}
		}

		available = {
			NOT = { is_in_faction_with = JAP }
		}

		cost = 25
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			USA = { has_completed_focus = USA_Embargo_Japan }
		}

		complete_effect = {
			add_opinion_modifier = { target = JAP modifier = embargo }

			if = {
				limit = { original_tag = ENG
					MAL = { is_subject_of = ENG } }
				MAL = { add_opinion_modifier = { target = JAP modifier = embargo } }
			}

			if = {
				limit = { original_tag = ENG
					RAJ = { is_subject_of = ENG } }
				RAJ = { add_opinion_modifier = { target = JAP modifier = embargo } }
			}

			if = {
				limit = { original_tag = ENG
					SAF = { is_subject_of = ENG } }
				SAF = { add_opinion_modifier = { target = JAP modifier = embargo } }
			}

			if = {
				limit = { original_tag = HOL
					INS = { is_subject_of = HOL } }
				INS = { add_opinion_modifier = { target = JAP modifier = embargo } }
			}

		}
	}

	USA_trade_embargo_against_germany = {

		icon = generic_prepare_civil_war


	USA_mexican_intervention = {
		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			OR = {
				MEX = { has_government = fascism }
				MEX = { has_government = communism }
				MEX = { fascism > 0.4 }
				MEX = { communism > 0.4 }
			}
		}

		cost = 0
		fire_only_once = yes

		ai_will_do = {
			factor = 200
			modifier = {
				#EAI
				factor = 0
				is_historical_focus_on = yes
				AND = {
					NOT = { MEX = { has_government = fascism } }
					NOT = { MEX = { has_government = communism } }
				}
			}
		}

		visible = {
			has_government = democratic
		}

		complete_effect = {
			create_wargoal = {
				type = puppet_wargoal_focus
				target = MEX
			}
		}

	}


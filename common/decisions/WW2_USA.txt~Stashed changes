USA_ai_buff = {

	USA_free_politics_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {

		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			NOT = { date < 1937.1.1 }
		}

		complete_effect = {
			add_political_power = 300
		}
	}

	USA_free_politics_act2 = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {

		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			NOT = { date < 1938.1.1 }
		}

		complete_effect = {
			add_political_power = 300
		}
	}

	USA_free_conscription_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			threat > 0.35
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			NOT = { has_completed_focus = USA_Selective_Training_and_Service_Act }
		}

		complete_effect = {
			complete_national_focus = USA_Selective_Training_and_Service_Act
		}
	}

	USA_free_marines_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			JAP = { has_added_tension_amount > 20 }
			threat > 0.6
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_Increase_Military_Funding
			NOT = { has_completed_focus = USA_Expand_the_Marines }
		}

		complete_effect = {
			complete_national_focus = USA_Expand_the_Marines
		}
	}

	USA_free_guards_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			OR = {
				JAP = { has_added_tension_amount > 5 }
				GER = { has_added_tension_amount > 5 }
				threat > 0.2
				has_war = yes
			}
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_Increase_Military_Funding
			NOT = { has_completed_focus = USA_Expand_the_Guard }
		}

		complete_effect = {
			complete_national_focus = USA_Expand_the_Guard
		}
	}

	USA_free_mobilization_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			has_war = yes
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_Two_Ocean_Navy_Act
			has_completed_focus = USA_Expand_Pearl_Harbor
			NOT = { has_completed_focus = USA_War_Mobilization }
		}

		complete_effect = {
			complete_national_focus = USA_War_Mobilization
		}
	}

	USA_free_lend_lease_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			threat > 0.4
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			NOT = { has_completed_focus = USA_Lend_Lease }
		}

		complete_effect = {
			complete_national_focus = USA_Lend_Lease
		}
	}


	USA_free_destroyers_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			country_exists = GER
			is_puppet = no
			ENG = {
				NOT = { is_in_faction_with = GER }
			}
			ENG = {
				has_war_with = GER
			}
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_Lend_Lease
			NOT = { has_completed_focus = USA_destroyers_for_bases }
		}

		complete_effect = {
			complete_national_focus = USA_destroyers_for_bases
		}
	}

	USA_free_british_assistance_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			ENG = { is_ai = no }
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_destroyers_for_bases
			NOT = { has_completed_focus = USA_British_Loans }
		}

		complete_effect = {
			complete_national_focus = USA_British_Loans
		}
	}

	USA_free_canada_act = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			OR = {
				ENG = { has_capitulated = yes }
				ENG = { is_puppet = yes }
				NOT = { country_exists = ENG }
				ENG = { has_government = fascism }
				CAN = { has_government = fascism }
				CAN = { has_government = communism }
			}
		}

		cost = 0
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			is_ai = yes
			has_completed_focus = USA_Proclaim_American_Neutrality
			NOT = { has_completed_focus = USA_Secure_Canada }
		}

		complete_effect = {
			complete_national_focus = USA_Secure_Canada
		}
	}
}






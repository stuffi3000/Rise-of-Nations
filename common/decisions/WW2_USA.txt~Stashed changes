	USA_direct_aid_i = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = USA
		}

		available = {
			SOV = { has_defensive_war_with = GER }

		}

		visible = {
			USA = { has_completed_focus = USA_Open_Up_the_American_Arsenal }
			has_dlc = "Waking the Tiger"
		}

		cost = 75
		days_remove = 180

		fire_only_once = no
		ai_will_do = {
			factor = 250
		}

		modifier = {
			civilian_factory_use = 15
		}

		complete_effect = {
			SOV = { add_offsite_building = { type = arms_factory level = 5 } add_offsite_building = { type = industrial_complex level = 5 } }
		}

		remove_effect = {
			SOV = { add_offsite_building = { type = arms_factory level = -5 } add_offsite_building = { type = industrial_complex level = -5 } }
		}

	}


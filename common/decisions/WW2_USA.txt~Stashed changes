foreign_support = {
	USA_support_the_anti_fascist_war = {

		icon = generic_industry

		allowed = {
			original_tag = USA
		}

		available = {
			date > 1920.01.01
			any_other_country = {
				#has_completed_focus = CHI_mission_to_the_us
				has_war_with = JAP
			}
			JAP = { has_government = fascism }
			ROOT = { has_government = democratic }
		}

		visible = {
			date > 1920.01.01
			any_other_country = {
				#has_completed_focus = CHI_mission_to_the_us
				has_war_with = JAP
			}
			JAP = { has_government = fascism }
			ROOT = { has_government = democratic }
		}

		cost = 25
		ai_will_do = {
			factor = 50
			modifier = {
				factor = 3
				any_other_country = {
					#has_completed_focus = CHI_mission_to_the_us
					has_idea = CHI_soong_mei_ling
					surrender_progress > 0.2
					has_defensive_war = yes
				}
			}

			modifier = {
				factor = 0.5
				has_war_support > 0.6
			}

			modifier = {
				factor = 2
				has_war_with = JAP
			}
		}

		fire_only_once = no

		modifier = {
			civilian_factory_use = 5
		}

		days_remove = 180

		complete_effect = {
			random_country = {
				limit = {
					#has_completed_focus = CHI_mission_to_the_us
					has_war_with = JAP
					THIS = { has_government = democratic }
				}
				add_offsite_building = { type = arms_factory level = 3 }
				set_country_flag = USA_support_the_anti_fascist_war
			}
		}

		remove_effect = {
			add_war_support = 0.05
			random_country = {
				limit = {
					if = {
						limit = {
							NOT = { any_other_country = { has_country_flag = USA_support_the_anti_fascist_war } }
						}
						#has_completed_focus = CHI_mission_to_the_us
						else = { has_country_flag = USA_support_the_anti_fascist_war }
					}
				}
				add_offsite_building = { type = arms_factory level = -3 }
			}
		}
	}


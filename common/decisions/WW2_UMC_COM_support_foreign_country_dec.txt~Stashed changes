	### JAP to RAJ
	COM_sfc_JAP_to_RAJ_dec = {
		icon = generic_prepare_civil_war
		allowed = {
			original_tag = JAP
		}
		available = {
			has_completed_focus = JAP_RAJ_support_the_anti_british_resistance
		}
		visible = {
			RAJ = {
				exists = yes
				OR = {
					has_civil_war = yes
					has_war_with = ENG
				}
			}
		}
		cost = 50
		days_remove = 90
		fire_only_once = yes
		modifier = {
			industrial_capacity_factory = -0.05
		}
		complete_effect = {
			army_experience = 5
			RAJ = {
				add_war_support = 0.025
				add_equipment_to_stockpile = {
					type = infantry_equipment
					amount = 2500
					producer = JAP
				}
				add_equipment_to_stockpile = {
					type = support_equipment_1
					amount = 25
					producer = JAP
				}
				add_equipment_to_stockpile = {
					type = artillery_equipment
					amount = 25
					producer = JAP
				}
				custom_effect_tooltip = COM_sfc_JAP_to_RAJ_dec_EXPL
				country_event = umc_sfc.40
			}
		}
		remove_effect = {
			add_political_power = 25
			add_war_support = 0.01
			army_experience = 5
		}
		ai_will_do = {
			factor = 500
		}
	}
	COM_sfc_JAP_to_RAJ_attache_dec = {
		icon = generic_operation
		allowed = {
			original_tag = JAP
		}
		available = {
			has_completed_focus = JAP_RAJ_support_the_anti_british_resistance
		}
		visible = {
			RAJ = {
				exists = yes
				OR = {
					has_civil_war = yes
					has_war_with = ENG
				}
			}
		}
		cost = 50
		fire_only_once = yes
		complete_effect = {
			country_event = umc_sfc.45
		}
		ai_will_do = {
			factor = 500
		}
	}
}

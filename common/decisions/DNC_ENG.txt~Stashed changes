political_actions = {
	Monarchist_coup_in_FRA_timer = {
		allowed = {
			OR = {
				tag = FRA
				tag = ENG
			}
		}
		available = {
			FRA = {
				has_government = neutrality
			}
		}
		activation = {
			ENG = {
				has_completed_focus = ENGDNC_monarchist_coup_in_france
			}
			FRA = {
				NOT = {
					has_government = neutrality
				}
			}
			date < 1910.1.1
		}
		is_good = no
		days_mission_timeout = 190
		timeout_effect = {
			set_global_flag = FRA_AFDNC_coup
			hidden_effect = {
				if = {
					limit = { tag = FRA }
					start_civil_war = {
						ideology = neutrality
						size = 0.55
					}
				}
				random_country = {
					limit = {
						original_tag = FRA
						has_government = neutrality
					}
					add_equipment_to_stockpile = {
						type = infantry_equipment
						amount = weapons_sent_by_ENG
						producer = ENG
					}
					add_political_power = pp_sent_by_ENG
				}
			}
			effect_tooltip = {
				FRA = {
					start_civil_war = {
						ideology = neutrality
					}
				}
			}
		}
		complete_effect = {

		}
	}
	ENGDNC_cede_power = {
		allowed = {
			tag = FRA
		}
		visible = {
			has_active_mission = Monarchist_coup_in_FRA_timer
			date < 1910.1.1
		}
		available = {
			neutrality > 0.35
		}
		complete_effect = {
			set_politics = {
				ruling_party = neutrality
				elections_allowed = no
			}
		}
	}
	ENGDNC_Arm_AF = {
		allowed = {
			tag = ENG
		}
		visible = {
			has_completed_focus = ENGDNC_arm_action_francaise
			date < 1910.1.1
			NOT = {
				has_global_flag = FRA_AFDNC_coup
			}

		}
		available = {
			has_equipment = {
				infantry_equipment > 1000
			}
		}
		complete_effect = {
			custom_effect_tooltip = equipment_will_be_sent_tt
			add_equipment_to_stockpile = {
				type = infantry_equipment
				amount = -1000
			}
			add_to_variable = { weapons_sent_by_ENG = 1000 }
		}
		days_re_enable = 30
	}
	ENGDNC_fund_AF = {
		allowed = {
			tag = ENG
		}
		visible = {
			has_completed_focus = ENGDNC_fund_action_francaise
			date < 1910.1.1
			NOT = {
				has_global_flag = FRA_AFDNC_coup
			}

		}
		cost = 50
		complete_effect = {
			custom_effect_tooltip = funds_will_be_sent_tt

			add_to_variable = { pp_sent_by_ENG = 50 }
		}
		days_re_enable = 30
	}
}

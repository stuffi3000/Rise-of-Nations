}

PRC_dcat_pacify_enemy_guerrilla_cell = {
	icon = category_infiltration

	visible = {
		date > 1932.12.31
		date < 1949.1.1
		any_state = {
			is_controlled_by = ROOT
			has_state_flag = KMT_PRC_state_cell_detected
			owner = {
				has_war_with = ROOT
			}
		}
		is_chinese_nations = no
		NOT = {
			is_subject_of = PRC
			tag = PRC
		}
	}

	allowed = {
		original_tag = PRC
		date > 1932.12.31
		date < 1949.1.1
	}
}


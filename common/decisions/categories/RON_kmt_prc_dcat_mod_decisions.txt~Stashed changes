PRC_dcat_frontline_behind_enemy_line = {
	icon = category_infiltration

	allowed = {
		original_tag = PRC
		date > 1932.12.31
		date < 1949.1.1
	}

	visible = {
		date > 1932.12.31
		date < 1949.1.1
		has_completed_focus = PRC_front_line_behind_enemy_line
		is_ai = no
	}

	allowed = {
	}
}


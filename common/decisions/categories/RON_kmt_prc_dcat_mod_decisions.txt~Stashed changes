PRC_dcat_chinese_civil_war_decide_capital = {
	icon = generic_propaganda

	visible = {
		date > 1932.12.31
		date < 1949.1.1
		NOT = { has_country_flag = prc_capital_decided }
		is_subject = no
		tag = PRC
		OR = {
			AND = {
				OR = {
					has_global_flag = PRC_restart_CHI_war
					CHI = {
						OR = {
							is_puppet_of = JAP
							is_in_faction_with = JAP
						}
					}
				}
				CHI = {
					exists = yes
				}
			}
			CHI = {
				exists = no
			}
		}
	}

	allowed = {
		original_tag = PRC
		date > 1932.12.31
		date < 1949.1.1
	}
}

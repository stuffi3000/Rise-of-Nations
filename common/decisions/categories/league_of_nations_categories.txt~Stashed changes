league_of_nations_category = {

	icon = generic_formable_nations

	picture = GFX_league_of_nations_icon

	allowed = {
		date > 1920.1.1
		date < 1949.1.1
	}

	visible = {
		NOT = { has_global_flag = league_of_nations_dissolved }
		NOT = {
			any_country = {
				has_country_flag = leader_of_league_of_nations
			}
		}
	}
}

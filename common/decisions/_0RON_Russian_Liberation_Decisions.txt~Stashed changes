Russian_National_Movements = {
	RNM_Prague_Manifesto = {
		icon = RNM_WHR
		fire_only_once = yes
		allowed = {
			original_tag = GER
		}
		available = {
			is_subject = no
			any_controlled_state = {
				is_core_of = RUS
				is_controlled_by = ROOT
			}

			has_war_with = SOV

		}
		visible = {
			original_tag = GER
			NOT = { country_exists = WHR }
			date > 1932.12.31
			date < 1949.1.1
		}
		ai_will_do = {
			factor = 0
		}
		complete_effect = {
			set_country_flag = RNM_Prague_Manifesto_Flag
			country_event = RNM_creationevents.1
		}
	}


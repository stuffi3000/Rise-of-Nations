SOV_munich_crisis = {

	SOV_offer_our_help_to_cze = {

		icon = generic_prepare_civil_war

		allowed = {
			original_tag = SOV
		}

		available = {
			#has_war_support > 0.7
			#surrender_progress > 0.3
			#SOV = { num_divisions < 300 }
		}

		cost = 0
		fire_only_once = yes

		days_remove = 5

		ai_will_do = {
			factor = 200
		}

		#visible = {
		#	#SOV = { has_decision = SOV_deploy_emergency_divisions_v }
		#}

		remove_effect = {
			CZE = { country_event = { id = soviet.235 days = 1 } }
			add_political_power = -50
		}
	}


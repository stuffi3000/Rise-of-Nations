SOV_french_soviet_pact = {

	SOV_join_the_pact = {

		icon = generic_prepare_civil_war

		allowed = {

		}

		available = {
			OR = {
				has_war_with = GER
				AND = {
					has_opinion = {
						target = SOV
						value > 70
					}
					has_opinion = {
						target = FRA
						value > 70
					}
				}
				has_opinion = {
					target = GER
					value < -70
				}
			}
		}

		cost = 50
		fire_only_once = yes
		ai_will_do = {
			factor = 200
		}

		visible = {
			#SOV = { has_decision = SOV_deploy_emergency_divisions }
		}

		complete_effect = {
			#SOV = {	load_oob = "SOV_conscription_i" }
			#add_to_faction = "Pact of Marseille"
			FRA = { add_to_faction = ROOT }
		}
	}

}



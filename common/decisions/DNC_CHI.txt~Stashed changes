china_influence_sphere = {

	FRA_chinese_intervention = {
		complete_effect = {
			591 = { set_state_flag = recently_contested }

		}
		ai_will_do = {
			base = 10
			modifier = {

				factor = 16
				591 = { has_state_flag = recently_contested }

			}

		}
		cost = 50

		name = decision.influence.1
		icon = {
			key = GFX_FRA_influence
			trigger = {
				591 = { has_state_flag = FRA_influence }
			}

		}
		icon = {
			key = GFX_JAP_influence
			trigger = {
				591 = { has_state_flag = JAP_influence }
			}

		}
		icon = {
			key = GFX_GER_influence
			trigger = {
				591 = { has_state_flag = GER_influence }
			}

		}
		icon = {
			key = GFX_ENG_influence
			trigger = {
				591 = { has_state_flag = ENG_influence }
			}

		}
		icon = {
			key = GFX_RUE_influence
			trigger = {
				591 = { has_state_flag = RUE_influence }
			}

		}


		visible = { NOT = { QIN = { has_country_flag = splitted } } }

		available = {

			country_exists = QIN
			custom_trigger_tooltip = {
				tooltip = decision.influence.tooltip
				QIN = { NOT = { has_country_flag = splitted } }
				OR = {
					AND = {
						591 = {
							any_neighbor_state = {
								has_state_flag = FRA_influence

							}
						}
						OR = {
							NOT = {
								591 = { has_state_flag = FRA_influence }
							}
							591 = { has_state_flag = recently_contested }
						}
						tag = FRA
					}
					AND = {
						591 = {
							any_neighbor_state = {
								has_state_flag = JAP_influence

							}
						}
						OR = {
							NOT = {
								591 = { has_state_flag = JAP_influence }
							}
							591 = { has_state_flag = recently_contested }
						}
						tag = JAP
					}
					AND = {
						591 = {
							any_neighbor_state = {
								has_state_flag = GER_influence

							}
						}
						OR = {
							NOT = {
								591 = { has_state_flag = GER_influence }
							}
							591 = { has_state_flag = recently_contested }
						}
						tag = GER
					}
					AND = {
						591 = {
							any_neighbor_state = {
								has_state_flag = RUE_influence

							}
						}
						OR = {
							NOT = {
								591 = { has_state_flag = RUE_influence }
							}
							591 = { has_state_flag = recently_contested }
						}
						tag = RUE
					}
					AND = {
						591 = {
							any_neighbor_state = {
								has_state_flag = ENG_influence

							}
						}
						OR = {
							NOT = {
								591 = { has_state_flag = ENG_influence }
							}
							591 = { has_state_flag = recently_contested }
						}
						tag = ENG
					}
				}
			}
		}


		highlight_state_targets = { state = 591 }

		days_remove = 14
		on_map_mode = map_only
		##highlight_color_before_active = 3 #Orange
		##highlight_color_while_active = 2 #Green
		#highlight_color_before_active = 5
		##highlight_color_while_active = 4
		remove_effect = {
			591 = { clr_state_flag = recently_contested }
			give_resource_rights = { receiver = ROOT state = 591 }
			if = {
				limit = {
					tag = FRA
				}
				hidden_effect = {
					591 = { set_state_flag = FRA_influence }
					591 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 591 }
					591 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 591 }
					591 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 591 }
					591 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 591 }
				}
			}
			else_if = {
				limit = {
					tag = JAP
				}
				hidden_effect = {
					591 = { set_state_flag = JAP_influence }
					591 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 591 }
					591 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 591 }
					591 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 591 }
					591 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 591 }
				}
			}
			else_if = {
				limit = {
					tag = RUE
				}
				hidden_effect = {
					591 = { set_state_flag = RUE_influence }
					591 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 591 }
					591 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 591 }
					591 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 591 }
					591 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 591 }
				}
			}
			else_if = {
				limit = {
					tag = ENG
				}
				hidden_effect = {
					591 = { set_state_flag = ENG_influence }
					591 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 591 }
					591 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 591 }
					591 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 591 }
					591 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 591 }
				}
			}
			else_if = {
				limit = {
					tag = GER
				}
				hidden_effect = {
					591 = { set_state_flag = GER_influence }
					591 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 591 }
					591 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 591 }
					591 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 591 }
					591 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 591 }
				}
			}
		}


	}
	FRA_chinese_intervention2 = {
		complete_effect = {
			594 = { set_state_flag = recently_contested }

		}
		ai_will_do = {
			base = 10
			modifier = {

				factor = 16
				594 = { has_state_flag = recently_contested }

			}

		}

		cost = 50

		name = decision.influence.1
		icon = {
			key = GFX_FRA_influence
			trigger = {
				594 = { has_state_flag = FRA_influence }
			}

		}
		icon = {
			key = GFX_JAP_influence
			trigger = {
				594 = { has_state_flag = JAP_influence }
			}

		}
		icon = {
			key = GFX_GER_influence
			trigger = {
				594 = { has_state_flag = GER_influence }
			}

		}
		icon = {
			key = GFX_ENG_influence
			trigger = {
				594 = { has_state_flag = ENG_influence }
			}

		}
		icon = {
			key = GFX_RUE_influence
			trigger = {
				594 = { has_state_flag = RUE_influence }
			}

		}
		visible = { QIN = { NOT = { has_country_flag = splitted } } }
		available = {
			country_exists = QIN
			custom_trigger_tooltip = {
				tooltip = decision.influence.tooltip
				OR = {
					AND = {
						594 = {
							any_neighbor_state = {
								has_state_flag = FRA_influence

							}
						}
						OR = {
							NOT = {
								594 = { has_state_flag = FRA_influence }
							}
							594 = { has_state_flag = recently_contested }
						}
						tag = FRA
					}
					AND = {
						594 = {
							any_neighbor_state = {
								has_state_flag = JAP_influence

							}
						}
						OR = {
							NOT = {
								594 = { has_state_flag = JAP_influence }
							}
						}
						tag = JAP
					}
					AND = {
						594 = {
							any_neighbor_state = {
								has_state_flag = GER_influence

							}
						}
						OR = {
							NOT = {
								594 = { has_state_flag = GER_influence }
							}
							594 = { has_state_flag = recently_contested }
						}
						tag = GER
					}
					AND = {
						594 = {
							any_neighbor_state = {
								has_state_flag = RUE_influence

							}
						}
						OR = {
							NOT = {
								594 = { has_state_flag = RUE_influence }
							}
							594 = { has_state_flag = recently_contested }
						}
						tag = RUE
					}
					AND = {
						594 = {
							any_neighbor_state = {
								has_state_flag = ENG_influence

							}
						}
						OR = {
							NOT = {
								594 = { has_state_flag = ENG_influence }
							}
							594 = { has_state_flag = recently_contested }
						}
						tag = ENG
					}
				} }
		}

		highlight_state_targets = { state = 594 }
		on_map_mode = map_only
		days_remove = 14
		##highlight_color_before_active = 3 #Orange
		##highlight_color_while_active = 2 #Green
		#highlight_color_before_active = 5
		##highlight_color_while_active = 4
		remove_effect = {
			594 = { clr_state_flag = recently_contested }
			give_resource_rights = { receiver = ROOT state = 594 }
			if = {
				limit = {
					tag = FRA
				}
				hidden_effect = {
					594 = { set_state_flag = FRA_influence }
					594 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 594 }
					594 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 594 }
					594 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 594 }
					594 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 594 }
				}
			}
			else_if = {
				limit = {
					tag = JAP
				}
				hidden_effect = {
					594 = { set_state_flag = JAP_influence }
					594 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 594 }
					594 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 594 }
					594 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 594 }
					594 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 594 }
				}
			}
			else_if = {
				limit = {
					tag = RUE
				}
				hidden_effect = {
					594 = { set_state_flag = RUE_influence }
					594 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 594 }
					594 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 594 }
					594 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 594 }
					594 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 594 }
				}
			}
			else_if = {
				limit = {
					tag = ENG
				}
				hidden_effect = {
					594 = { set_state_flag = ENG_influence }
					594 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 594 }
					594 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 594 }
					594 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 594 }
					594 = { clr_state_flag = GER_influence }
					GER = { remove_resource_rights = 594 }
				}
			}
			else_if = {
				limit = {
					tag = GER
				}
				hidden_effect = {
					594 = { set_state_flag = GER_influence }
					594 = { clr_state_flag = FRA_influence }
					FRA = { remove_resource_rights = 594 }
					594 = { clr_state_flag = JAP_influence }
					JAP = { remove_resource_rights = 594 }
					594 = { clr_state_flag = ENG_influence }
					ENG = { remove_resource_rights = 594 }
					594 = { clr_state_flag = RUE_influence }
					RUE = { remove_resource_rights = 594 }
				}
			}
		}


	}

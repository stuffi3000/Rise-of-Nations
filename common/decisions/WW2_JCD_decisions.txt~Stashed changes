				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 716
			}
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 715
			}
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 328
			}

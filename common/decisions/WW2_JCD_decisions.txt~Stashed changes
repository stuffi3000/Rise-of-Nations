					custom_effect_tooltip = JAP_liberate_manchuria_alliance_tt
				}
			}
			if = {
				limit = {
					country_exists = event_target:WTT_communist_china
					NOT = { country_exists = event_target:WTT_current_china_leader }
				}
				event_target:WTT_communist_china = {
					transfer_state = 609
					transfer_state = 610
					transfer_state = 716
					transfer_state = 715
					transfer_state = 328

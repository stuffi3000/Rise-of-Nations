				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 717
			}
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 714
			}
		}

		visible = {
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				owns_state = 609
			}
			any_country = {
				OR = {
					is_puppet_of = JAP

				}
				owns_state = 745
			}
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				owns_state = 761
			}
			has_completed_focus = JCD_liberate_manchuria
		}

		fire_only_once = yes


		modifier = {

		}

		days_remove = 0
		days_re_enable = 0

		cost = 0

		ai_will_do = {
			factor = 1
		}

		complete_effect = {
			if = {
				limit = {
					country_exists = event_target:WTT_current_china_leader
				}
				event_target:WTT_current_china_leader = {
					transfer_state = 609
					transfer_state = 610
					transfer_state = 716
					transfer_state = 715
					transfer_state = 328
					transfer_state = 717
					transfer_state = 714

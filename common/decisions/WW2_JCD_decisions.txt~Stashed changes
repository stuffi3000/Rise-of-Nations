#Category allowed for JCD
JAP_colonial_independence = {
	JAP_return_manchuria = {

		icon = generic_independence

		allowed = {
			original_tag = JAP
		}

		available = {
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 609
			}
			any_country = {
				OR = {
					is_puppet_of = JAP
					original_tag = JAP
				}
				has_full_control_of_state = 610
			}

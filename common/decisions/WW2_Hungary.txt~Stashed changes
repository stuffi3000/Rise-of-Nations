					if = {
						limit = { has_cosmetic_tag = HUN_DNF }
						SZE = { set_cosmetic_tag = SZE_HUN_DNF }
					}
					if = {
						limit = { has_cosmetic_tag = HUN_USH }
						SZE = { set_cosmetic_tag = SZE_HUN_USH }
					}
					if = {
						limit = { has_cosmetic_tag = HUN_POL }
						SZE = { set_cosmetic_tag = SZE_HUN_POL }
					}
				}
			}
		}
	}


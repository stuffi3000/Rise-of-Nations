			owns_state = 70    #Western Slovakia
		}

		visible = {
			has_country_flag = HUN_slovakia_is_state
			70 = { NOT = { has_state_flag = HUN_is_state } }
			owns_state = 70    #Western Slovakia
		}

		cost = 0
		fire_only_once = yes
		days_remove = 7

		ai_will_do = {
			factor = 0
		}
		remove_effect = {
			70 = {
				#Western Slovakia
				if = {
					limit = { is_owned_by = ROOT }
					add_state_modifier = {
						modifier = {
							local_non_core_manpower = 0.5
						}
					}
				}
			}
		}
	}


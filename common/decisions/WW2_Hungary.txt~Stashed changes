			custom_effect_tooltip = HUN_the_arpad_line_forts
			73 = {
				hidden_effect = {
					set_building_level = {
						type = bunker
						level = 5
						province = {
							all_provinces = yes
							limit_to_border = no
							level < 5
						}
					}
				}
			}
			if = {
				limit = {
					SOV = {
						owns_state = 89 #Stanislawow
					}
				}
				SOV = {
					add_opinion_modifier = { target = ROOT modifier = HUN_arpad_line }
				}
			}
			set_country_flag = HUN_arpad_line_done
			news_event = { id = hun_news.33 }
		}
	}


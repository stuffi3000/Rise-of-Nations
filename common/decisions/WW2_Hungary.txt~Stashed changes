				#Zaolzie
				if = {
					limit = { is_owned_by = ROOT }
					set_state_flag = HUN_is_state
					add_state_modifier = {
						modifier = {
							local_non_core_manpower = 0.5
						}
					}
				}
			}
			88 = {
				#Krakow
				if = {
					limit = { is_owned_by = ROOT }
					set_state_flag = HUN_is_state
					add_state_modifier = {
						modifier = {
							local_non_core_manpower = 0.5
						}
					}
				}
			}
			91 = {
				#Lwow

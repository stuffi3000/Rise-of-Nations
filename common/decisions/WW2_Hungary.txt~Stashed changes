	HUN_start_the_integration_of_banat = {
		icon = generic_decision

		available = {
			owns_state = 82        #Banat
			NOT = { 82 = { is_core_of = ROOT } }    #Banat
		}

		visible = {
			owns_state = 82        #Banat
			NOT = { 82 = { is_core_of = ROOT } }    #Banat
			NOT = { 82 = { has_state_flag = HUN_integration_abandoned } }    #Banat
		}

		highlight_states = {
			state = 82        #Banat
		}

		#on_map_mode = map_only

		cost = 5

		complete_effect = {
			add_05_conscription_resistance = yes
			82 = { #Banat
				add_core_of = ROOT
				custom_effect_tooltip = HUN_integration_of_the_state_will_start
				hidden_effect = { set_state_flag = HUN_is_being_integrated }
			}
		}

		ai_will_do = {
			factor = 10
		}
	}


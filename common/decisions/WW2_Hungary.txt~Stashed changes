political_actions = {

	HUN_move_capital_to_vienna = {

		icon = generic_political_discourse

		available = {
			owns_state = 43        #Budapest
			owns_state = 4        #Vienna
		}

		cost = 10
		fire_only_once = yes
		days_remove = 7

		allowed = {
			original_tag = HUN
		}

		visible = {
			is_ahu = yes
			owns_state = 43        #Budapest
			owns_state = 4        #Vienna
		}
		ai_will_do = {
			factor = 0
		}
		remove_effect = {
			set_capital = 4
			if = {
				limit = { has_cosmetic_tag = HUN_AHU }
				custom_effect_tooltip = HUN_will_change_map_color_to_white
				hidden_effect = { set_cosmetic_tag = HUN_AHU_WHITE }
			}
			if = {
				limit = { has_cosmetic_tag = HUN_AHF }
				custom_effect_tooltip = HUN_will_change_map_color_to_white
				hidden_effect = { set_cosmetic_tag = HUN_AHF_WHITE }
			}
		}
	}

	HUN_reinstate_croatian_military_leaders = {

		icon = generic_army_support

		allowed = {
			original_tag = HUN
		}

		available = {
			#Slavonia
			109 = {
				is_core_of = ROOT
				is_owned_by = ROOT
			}
			#Croatia
			803 = {
				is_core_of = ROOT
				is_owned_by = ROOT
			}
			#Pag
			813 = {
				is_core_of = ROOT
				is_owned_by = ROOT
			}
			#Dalmatia
			103 = {
				is_core_of = ROOT
				is_owned_by = ROOT
			}
		}

		cost = 30
		fire_only_once = yes
		days_remove = 7

		visible = {
			is_ahu = yes
			owns_state = 803    #Croatia
		}
		ai_will_do = {
			factor = 0
		}

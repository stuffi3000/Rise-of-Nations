							tag = RUT
							is_subject_of = ROOT
						}
					}
				}
			}
		}

		allowed = {
			original_tag = HUN
		}

		visible = {
			SLO = {
				exists = yes
				is_subject_of = ROOT
				owns_state = 70        #Western Slovakia
				owns_state = 71        #Eastern Slovakia
				OR = {
					NOT = { owns_state = 73 }    #Carpathian Ruthenia
					NOT = { owns_state = 818 }    #Sobrance
					NOT = { owns_state = 802 }    #Berehovo
				}
			}
			818 = { #Sobrance
				OWNER = {
					OR = {
						tag = ROOT
						tag = SLO
						AND = {
							tag = RUT
							is_subject_of = ROOT
						}
					}
				}
			}

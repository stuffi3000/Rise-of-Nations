	HUN_grant_autonomy_to_the_szeklers = {

		icon = generic_nationalism

		available = {
			819 = { #Szeklerland
				OWNER = {
					OR = {
						tag = ROOT
						is_subject_of = ROOT
					}
				}
			}
		}

		cost = 10
		days_re_enable = 0

		allowed = {
			OR = {
				original_tag = HUN
				original_tag = ROM
			}
		}

		visible = {
			OR = {
				tag = HUN
				tag = ROM
			}
			is_not_ahu = yes
			SZE = { exists = no }
			819 = { #Szeklerland
				OWNER = {
					OR = {
						tag = ROOT
						is_subject_of = ROOT
					}
				}
			}
		}
		ai_will_do = {
			factor = 0
		}
		complete_effect = {
			add_political_power = 100
			SZE = { transfer_state = 819 }    #Szeklerland
			if = {
				limit = { has_dlc = "Together for Victory" }
				set_autonomy = {
					target = SZE
					autonomous_state = autonomy_integrated_puppet
					freedom_level = 0.5
				}
				else = { puppet = SZE }
			}
			if = { limit = { 819 = { is_core_of = ROOT } } remove_state_core = 819 }                #Szeklerland
			819 = { set_state_flag = HUN_claim_forbidden clr_state_flag = HUN_is_claimed_by_HUN }    #Szeklerland
		}
	}


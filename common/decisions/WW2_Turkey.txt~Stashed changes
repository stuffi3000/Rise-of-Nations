				name = "Mustafa Kemal Atatürk"
				picture = "Portrait_Turkey_Mustafa_Kemal_Ataturk_Military.dds"
				traits = { brilliant_strategist war_hero infantry_officer trickster desert_fox hill_fighter }
				skill = 4
				id = 59
				attack_skill = 4
				defense_skill = 4
				planning_skill = 4
				logistics_skill = 3
			}
			add_war_support = 0.1
			add_stability = -0.05
		}
	}

